            <method name="beforeOpen"><![CDATA[this.queryText=
" SELECT PP.ENT_NUMERO_PROSA_EMI NUMERO_ENTIDAD "+
"       ,(SELECT DISTINCT VBE.BU_TX_PARENT FROM PMADMIN.VW_BUS_EMI VBE WHERE VBE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI AND ROWNUM = 1) BU "+
"       ,PE.DESCRIPCION "+
"       ,DECODE(PP.RUTEO,'1','PROSA','2','TSYS','3','VISA','4','MASTERCARD', 'PROSA') RUTEO "+
"       ,PTT.DESCRIPCION TTR_DESCRIPCION "+
"       ,DECODE(PP.ARCH_NUMERO, 5, 'P . O . S .','SUCURSAL') PRO_DESCRIPCION "+
"       ,SUM(PP.NUMERO_TRANSACCIONES) TOTAL_REGS "+
"       ,SUM(PP.TOTAL_IMPORTE) IMPORTE_TOTAL_TRANSAC "+
"       ,SUM(CASE   "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PP.IMPORTE_COMISION_COMERCIO,0) "+ 
"             ELSE NVL(PP.IMPORTE_COMISION_INTERCAMB,0) "+ 
"        END) IMPORTE_COMISION_CALCULADO "+
"       ,SUM(CASE   "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PP.IVA_COMISION_COMERCIO,0) "+ 
"             ELSE NVL(PP.IVA_COMISION_INTERCAMB,0) "+ 
"        END) IVA_COMISION_CALCULADO "+
"       ,SUM(CASE   "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_COMERCIO,0)-NVL(PP.IVA_COMISION_COMERCIO,0) "+ 
"             ELSE PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_INTERCAMB ,0)-NVL(PP.IVA_COMISION_INTERCAMB,0) "+ 
"        END) NETO "+
"       ,SUM(CASE "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -PP.TOTAL_IMPORTE "+
"             ELSE PP.TOTAL_IMPORTE "+
"        END) SIGNO_TOTAL_TRANSAC "+
"       ,SUM(CASE   "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PP.TTR_NUMERO IN (20,21) THEN -NVL(PP.IMPORTE_COMISION_COMERCIO,0) "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PP.IMPORTE_COMISION_COMERCIO,0) "+ 
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -NVL(PP.IMPORTE_COMISION_INTERCAMB,0) "+ 
"             ELSE NVL(PP.IMPORTE_COMISION_INTERCAMB,0) "+
"        END) SIGNO_COMISION "+
"       ,SUM(CASE   "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PP.TTR_NUMERO IN (20,21) THEN -NVL(PP.IVA_COMISION_COMERCIO,0) "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PP.IVA_COMISION_COMERCIO,0) "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -NVL(PP.IVA_COMISION_INTERCAMB,0) "+
"             ELSE NVL(PP.IVA_COMISION_INTERCAMB,0) "+
"        END) SIGNO_IVA "+
"       ,SUM(CASE   "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PP.TTR_NUMERO IN (20,21) THEN -(PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_COMERCIO,0)-NVL(PP.IVA_COMISION_COMERCIO,0)) "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_COMERCIO,0)-NVL(PP.IVA_COMISION_COMERCIO,0) "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -(PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_INTERCAMB ,0)-NVL(PP.IVA_COMISION_INTERCAMB,0)) "+
"             ELSE PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_INTERCAMB ,0)-NVL(PP.IVA_COMISION_INTERCAMB,0) "+ 
"        END) SIGNO_NETO "+
" FROM PMADMIN.PRSA_POSICIONES PP "+
" INNER JOIN PMADMIN.PRSA_ENTIDADES PE "+
"     ON PE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI "+
" INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT "+
"     ON PTT.NUMERO = PP.TTR_NUMERO "+
" WHERE PP.ENT_NUMERO_PROSA_EMI IN ("+params["bancoEmi"]+") "+
" AND PP.TTR_NUMERO IN ("+params["typeTrans"]+") "+
" AND PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+
" GROUP BY PP.ENT_NUMERO_PROSA_EMI "+
"       ,PE.DESCRIPCION "+
"       ,DECODE(PP.RUTEO,'1','PROSA','2','TSYS','3','VISA','4','MASTERCARD', 'PROSA') "+
"       ,PTT.DESCRIPCION "+
"       ,DECODE(PP.ARCH_NUMERO, 5, 'P . O . S .','SUCURSAL') "+
" UNION ALL "+
//--Modificacion: HUB Marca de inicio SAS-LERC P-06-0527-12 11/08/2014 
" SELECT PP.ENT_NUMERO_PROSA_EMI NUMERO_ENTIDAD  "+
"       ,(SELECT DISTINCT VBE.BU_TX_PARENT FROM PMADMIN.VW_BUS_EMI VBE WHERE VBE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI AND ROWNUM = 1) BU  "+
"       ,PE.NOMBRE  "+
"       ,DECODE(PP.RUTEO,'1','PROSA','2','TSYS','3','VISA','4','MASTERCARD', 'PROSA') RUTEO  "+
"       ,PTT.DESCRIPCION TTR_DESCRIPCION  "+
"       ,DECODE(PP.ARCH_NUMERO, 5, 'P . O . S .','SUCURSAL') PRO_DESCRIPCION  "+
"       ,SUM(PP.NUMERO_TRANSACCIONES) TOTAL_REGS  "+
"       ,SUM(PP.TOTAL_IMPORTE) IMPORTE_TOTAL_TRANSAC  "+
"       ,SUM(CASE    "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PP.IMPORTE_COMISION_COMERCIO,0)   "+
"             ELSE NVL(PP.IMPORTE_COMISION_INTERCAMB,0)   "+
"        END) IMPORTE_COMISION_CALCULADO  "+
"       ,SUM(CASE    "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PP.IVA_COMISION_COMERCIO,0)   "+
"             ELSE NVL(PP.IVA_COMISION_INTERCAMB,0)   "+
"        END) IVA_COMISION_CALCULADO  "+
"       ,SUM(CASE    "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_COMERCIO,0)-NVL(PP.IVA_COMISION_COMERCIO,0)   "+
"             ELSE PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_INTERCAMB ,0)-NVL(PP.IVA_COMISION_INTERCAMB,0)   "+
"        END) NETO  "+
"       ,SUM(CASE  "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -PP.TOTAL_IMPORTE  "+
"             ELSE PP.TOTAL_IMPORTE  "+
"        END) SIGNO_TOTAL_TRANSAC  "+
"       ,SUM(CASE    "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PP.TTR_NUMERO IN (20,21) THEN -NVL(PP.IMPORTE_COMISION_COMERCIO,0)  "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PP.IMPORTE_COMISION_COMERCIO,0)   "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -NVL(PP.IMPORTE_COMISION_INTERCAMB,0)   "+
"             ELSE NVL(PP.IMPORTE_COMISION_INTERCAMB,0)  "+
"        END) SIGNO_COMISION  "+
"       ,SUM(CASE    "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PP.TTR_NUMERO IN (20,21) THEN -NVL(PP.IVA_COMISION_COMERCIO,0)  "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PP.IVA_COMISION_COMERCIO,0)  "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -NVL(PP.IVA_COMISION_INTERCAMB,0)  "+
"             ELSE NVL(PP.IVA_COMISION_INTERCAMB,0)  "+
"        END) SIGNO_IVA"+  
"       ,SUM(CASE    "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PP.TTR_NUMERO IN (20,21) THEN -(PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_COMERCIO,0)-NVL(PP.IVA_COMISION_COMERCIO,0))  "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_COMERCIO,0)-NVL(PP.IVA_COMISION_COMERCIO,0)  "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -(PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_INTERCAMB ,0)-NVL(PP.IVA_COMISION_INTERCAMB,0))  "+
"             ELSE PP.TOTAL_IMPORTE-NVL(PP.IMPORTE_COMISION_INTERCAMB ,0)-NVL(PP.IVA_COMISION_INTERCAMB,0)   "+
"        END) SIGNO_NETO  "+
" FROM CHP.TBL_CHP_POSICIONES@LG_PMTU_PMTWEB_SCP_CHP PP  "+
" INNER JOIN HUB.TBL_HUB_ENTIDADES@LG_PMTU_PMTWEB_PWS_HUB PE  "+
"     ON PE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI  "+
" INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT  "+
"     ON PTT.NUMERO = PP.TTR_NUMERO  "+
" WHERE PP.ENT_NUMERO_PROSA_EMI IN ("+params["bancoEmi"]+")"+  
" AND PP.TTR_NUMERO IN ("+params["typeTrans"]+")"+  
" AND PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY')"+  
" GROUP BY PP.ENT_NUMERO_PROSA_EMI"+  
"       ,PE.NOMBRE  "+
"       ,DECODE(PP.RUTEO,'1','PROSA','2','TSYS','3','VISA','4','MASTERCARD', 'PROSA')"+  
"       ,PTT.DESCRIPCION  "+
"       ,DECODE(PP.ARCH_NUMERO, 5, 'P . O . S .','SUCURSAL') "+
//--Modificacion: HUB Marca de fin SAS-LERC P-06-0527-12 11/08/2014 
" UNION ALL "+
" SELECT PTRV.PTRV_ENT_NUMERO_PROSA_EMI ENT_NUMERO_PROSA_EMI "+
"       ,(SELECT DISTINCT VBE.BU_TX_PARENT FROM PMADMIN.VW_BUS_EMI VBE WHERE VBE.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_EMI AND ROWNUM = 1) BU "+
"       ,PE.DESCRIPCION "+
"       ,'PROSA' RUTEO "+
"       ,DECODE(PTRV.PTRV_TTR_NUMERO,NULL,' ** SIN TIPO DE TRX ',PTT.DESCRIPCION) TTR_DESCRIPCION "+
"       ,'RECHAZOS' PRO_DESCRIPCION "+
"       ,COUNT(1) TOTAL_REGS "+
"       ,SUM(PTRV.PTRV_IMPORTE_TOTAL_TRANSAC) IMPORTE_TOTAL_TRANSAC "+
"       ,SUM(CASE   "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PTRV.PTRV_IMPORTE_CUOTA_COMERCIO,0) "+ 
"             ELSE NVL(PTRV.PTRV_IMP_CUOTA_INTERCAMBIO,0) "+ 
"        END) IMPORTE_COMISION_CALCULADO "+
"       ,SUM(CASE   "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PTRV.PTRV_IMPORTE_IVA_COMERCIO,0) "+ 
"             ELSE NVL(PTRV.PTRV_IMP_IVA_INTERCAMBIO,0) "+ 
"        END) IVA_COMISION_CALCULADO "+
"       ,SUM(CASE   "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-NVL(PTRV.PTRV_IMPORTE_CUOTA_COMERCIO,0)-NVL(PTRV.PTRV_IMPORTE_IVA_COMERCIO,0) "+ 
"             ELSE PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-NVL(PTRV.PTRV_IMP_CUOTA_INTERCAMBIO,0)-NVL(PTRV.PTRV_IMP_IVA_INTERCAMBIO,0) "+ 
"        END) NETO "+
"       ,SUM(CASE "+
"             WHEN PTRV.PTRV_TTR_NUMERO IN (20,21) THEN -PTRV.PTRV_IMPORTE_TOTAL_TRANSAC "+
"             ELSE PTRV.PTRV_IMPORTE_TOTAL_TRANSAC "+
"        END) IMPORTE_TOTAL_TRANSAC "+
"       ,SUM(CASE   "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PTRV.PTRV_TTR_NUMERO IN (20,21) THEN -NVL(PTRV.PTRV_IMPORTE_CUOTA_COMERCIO,0) "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PTRV.PTRV_IMPORTE_CUOTA_COMERCIO,0) "+
"             WHEN PTRV.PTRV_TTR_NUMERO IN (20,21) THEN -NVL(PTRV.PTRV_IMP_CUOTA_INTERCAMBIO,0) "+
"             ELSE NVL(PTRV.PTRV_IMP_CUOTA_INTERCAMBIO,0) "+ 
"        END) IMPORTE_COMISION_CALCULADO "+
"       ,SUM(CASE   "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PTRV.PTRV_TTR_NUMERO IN (20,21) THEN -NVL(PTRV.PTRV_IMPORTE_IVA_COMERCIO,0) "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN NVL(PTRV.PTRV_IMPORTE_IVA_COMERCIO,0) "+
"             WHEN PTRV.PTRV_TTR_NUMERO IN (20,21) THEN -NVL(PTRV.PTRV_IMP_IVA_INTERCAMBIO,0) "+
"             ELSE NVL(PTRV.PTRV_IMP_IVA_INTERCAMBIO,0) "+ 
"        END) IVA_COMISION_CALCULADO "+
"       ,SUM(CASE   "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) AND PTRV.PTRV_TTR_NUMERO IN (20,21) THEN -(PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-NVL(PTRV.PTRV_IMPORTE_CUOTA_COMERCIO,0)-NVL(PTRV.PTRV_IMPORTE_IVA_COMERCIO,0)) "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (SELECT VBA.NUMERO_PROSA FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.TIE_NUMERO IN (2)) THEN PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-NVL(PTRV.PTRV_IMPORTE_CUOTA_COMERCIO,0)-NVL(PTRV.PTRV_IMPORTE_IVA_COMERCIO,0) "+
"             WHEN PTRV.PTRV_TTR_NUMERO IN (20,21) THEN -(PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-NVL(PTRV.PTRV_IMP_CUOTA_INTERCAMBIO,0)-NVL(PTRV.PTRV_IMP_IVA_INTERCAMBIO,0)) "+
"             ELSE PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-NVL(PTRV.PTRV_IMP_CUOTA_INTERCAMBIO,0)-NVL(PTRV.PTRV_IMP_IVA_INTERCAMBIO,0) "+ 
"        END) NETO  "+
" FROM PMADMIN.PRSA_TXN_RECHAZADAS_VAL PTRV "+
" INNER JOIN PMADMIN.PRSA_ENTIDADES PE "+
"     ON PE.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
" LEFT JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT "+
"     ON PTT.NUMERO = PTRV.PTRV_TTR_NUMERO "+
" WHERE PTRV.PTRV_ENT_NUMERO_PROSA_EMI IN ("+params["bancoEmi"]+") "+
" AND PTRV.PTRV_TTR_NUMERO IN ("+params["typeTrans"]+") "+
" AND PTRV.PTRV_FECHA_PROCESO_TRANSAC BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+
" GROUP BY PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
"       ,PE.DESCRIPCION "+
"       ,DECODE(PTRV.PTRV_TTR_NUMERO,NULL,' ** SIN TIPO DE TRX ',PTT.DESCRIPCION) ";]]></method>
            <property name="queryText">SELECT ENT_NUMERO_PROSA_EMI NUMERO_ENTIDAD
,ENT.DESCRIPCION
,DECODE(TAC.RUTEO,'1','PROSA','2','TSYS','3','VISA','4','MASTERCARD', 'PROSA') RUTEO
,TTR.DESCRIPCION TTR_DESCRIPCION
,decode (tac.arch_numero, 5, 'P . O . S .','SUCURSAL')    PRO_DESCRIPCION
,COUNT(TAC.NUMERO) TOTAL_REGS
,SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0))        IMPORTE_TOTAL_TRANSAC
,SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0))    IMPORTE_COMISION_CALCULADO
,SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0))       IVA_COMISION_CALCULADO
,SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0))-(SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0))+SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0)))       NETO
,DECODE(TTR.NATURALEZA_CONTABLE,'-', SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)) * -1,SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)))           SIGNO_TOTAL_TRANSAC
,DECODE(TTR.NATURALEZA_CONTABLE,'-', SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0)) *  -1,SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0)))  SIGNO_COMISION
,DECODE(TTR.NATURALEZA_CONTABLE,'-', SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0)) * -1,SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0)))         SIGNO_IVA
,DECODE(TTR.NATURALEZA_CONTABLE,'-', (SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)) - 
                                     (SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0)) +
                                      SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0)))) * - 1,
                                      SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)) - 
                                     (SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0)) + SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0))))       SIGNO_NETO
FROM    pmadmin.PRSA_TXN_ACEPTADAS TAC
       ,pmadmin.PRSA_ENTIDADES ENT
       ,pmadmin.PRSA_TIPOS_TRANSACCION TTR
WHERE
TAC.FECHA_PROCESO_TRANSAC BETWEEN TO_DATE('06-02-2009','dd-MM-yyyy') AND TO_DATE('06-02-2009','dd-MM-yyyy')
AND TAC.ENT_NUMERO_PROSA_EMI = ENT.NUMERO_PROSA
AND TAC.TTR_NUMERO=TTR.NUMERO
AND (ENT.GCO_NUMERO != 3 OR ENT.GCO_NUMERO IS NULL) 
group by
 ENT_NUMERO_PROSA_EMI 
,ENT.DESCRIPCION
,tac.ruteo
,TTR.DESCRIPCION
,decode (tac.arch_numero,5,'P . O . S .','SUCURSAL')
,TTR.NATURALEZA_CONTABLE
UNION
SELECT
   ENT_NUMERO_PROSA_EMI NUMERO_ENTIDAD
  ,ENT.DESCRIPCION
  ,'PROSA' ruteo 
  ,DECODE(TAC.TTR_NUMERO,NULL,' ** SIN TIPO DE TRX ',TTR.DESCRIPCION) TTR_DESCRIPCION
  ,'RECHAZOS' PRO_DESCRIPCION
  ,COUNT(TAC.NUMERO) TOTAL_REGS
  ,SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)) IMPORTE_TOTAL_TRANSAC
  ,SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0))          IMPORTE_COMISION_CALCULADO
  ,SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0)) IVA_COMISION_CALCULADO
  ,SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)) -    
  (SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0)) +
   SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0))) NETO          
  ,DECODE(TTR.NATURALEZA_CONTABLE,'-',SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)) * - 1, SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0))) SIGNO_TOTAL_TRANSAC
  ,DECODE(TTR.NATURALEZA_CONTABLE,'-',SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0)) * - 1,SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0))) SIGNO_COMISION
  ,DECODE(TTR.NATURALEZA_CONTABLE,'-',SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0)) * -1,SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0))) SIGNO_IVA
  ,DECODE(TTR.NATURALEZA_CONTABLE,'-',(SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)) - 
                                      (SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0)) + 
                                       SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0)))) * -1,
                                       SUM(NVL(TAC.IMPORTE_TOTAL_TRANSAC ,0)) - (SUM(NVL(TAC.IMPORTE_CUOTA_INTERCAMBIO ,0)) + 
                                       SUM(NVL(TAC.IMPORTE_IVA_INTERCAMBIO,0)))) SIGNO_NETO
FROM    pmadmin.prsa_txn_rechazadas_val TAC
       ,pmadmin.PRSA_ENTIDADES ENT
       ,pmadmin.PRSA_TIPOS_TRANSACCION TTR
WHERE
TAC.FECHA_PROCESO_TRANSAC BETWEEN TO_DATE('06-02-2009','dd-MM-yyyy') AND TO_DATE('06-02-2009','dd-MM-yyyy')
AND TAC.ENT_NUMERO_PROSA_EMI IS NOT NULL
AND TAC.ENT_NUMERO_PROSA_EMI = ENT.NUMERO_PROSA
AND (ENT.GCO_NUMERO != 3 OR ENT.GCO_NUMERO IS NULL) 
group by
 ENT_NUMERO_PROSA_EMI 
,ENT.DESCRIPCION
,DECODE(TAC.TTR_NUMERO,NULL,' ** SIN TIPO DE TRX ',TTR.DESCRIPCION)
,TTR.NATURALEZA_CONTABLE
ORDER BY 1,2,3
</property>
