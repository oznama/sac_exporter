    <data-sets>
        <oda-data-set extensionID="com.genuitec.eclipse.reporting.oda.dataSet" name="PROSA" id="2514">
            <structure name="cachedMetaData">
                <list-property name="resultSet">
                    <structure>
                        <property name="position">1</property>
                        <property name="name">ENTIDAD_PADRE</property>
                        <property name="dataType">decimal</property>
                    </structure>
                    <structure>
                        <property name="position">2</property>
                        <property name="name">NOMBRE</property>
                        <property name="dataType">string</property>
                    </structure>
                    <structure>
                        <property name="position">3</property>
                        <property name="name">ENTRANTE</property>
                        <property name="dataType">decimal</property>
                    </structure>
                    <structure>
                        <property name="position">4</property>
                        <property name="name">SALIENTE</property>
                        <property name="dataType">decimal</property>
                    </structure>
                </list-property>
            </structure>
            <property name="dataSource">Data Source</property>
            <method name="beforeOpen"><![CDATA[this.queryText=
" SELECT DATOS2.ENTIDAD_PADRE "+
"       ,(SELECT PE.DESCRIPCION FROM PMADMIN.PRSA_ENTIDADES PE WHERE DECODE(DATOS2.ENTIDAD_PADRE,131,PE.NUMERO_PROSA,PE.ENTIDAD_PADRE) = DATOS2.ENTIDAD_PADRE AND ROWNUM = 1) NOMBRE "+
"       ,ROUND(SUM(DATOS2.ENTRANTE),2) ENTRANTE "+
"       ,ROUND(SUM(DATOS2.SALIENTE),2) SALIENTE "+
" FROM ( "+
" SELECT (SELECT DECODE(DATOS.ENTIDAD,131,DATOS.ENTIDAD,PE.ENTIDAD_PADRE) FROM PMADMIN.PRSA_ENTIDADES PE WHERE PE.NUMERO_PROSA = DATOS.ENTIDAD AND ROWNUM = 1) ENTIDAD_PADRE "+
"       ,SUM(CASE  "+
"             WHEN DATOS.LIQ = 2 THEN DATOS.NETO "+
"             ELSE 0 "+
"        END) ENTRANTE "+
"       ,SUM(CASE  "+
"             WHEN DATOS.LIQ = 3 THEN DATOS.NETO "+
"             ELSE 0 "+
"        END) SALIENTE "+
" FROM( "+
" SELECT 2 LIQ "+
"       ,VBA.ENTIDAD_PADRE ENTIDAD "+ 
"       ,PRT.PRT_TRAN_TYP TTR  "+
"       ,SUM(DECODE(PTT.NATURALEZA_CONTABLE, '-', -PRT.PRT_TRAN_AMT + PRT.PRT_INTCHG_FEE_AMT + PRT.PRT_INTCHG_IVA_AMT, PRT.PRT_TRAN_AMT - PRT.PRT_INTCHG_FEE_AMT - PRT.PRT_INTCHG_IVA_AMT)) NETO "+ 
"  FROM PMADMIN.PRSA_REJECTED_TXN PRT  "+
"       INNER JOIN PMADMIN.VW_BUS_EMI VBE  "+
"          ON PRT.PRT_ISS_BU = VBE.BU_TX_ISS  "+
"       INNER JOIN PMADMIN.VW_BUS_ACQ VBA  "+
"          ON PRT.PRT_ACQ_BU = VBA.BU_TX_ADQ  "+
"       INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT "+ 
"          ON PRT.PRT_TRAN_TYP = PTT.NUMERO "+ 
" WHERE PRT.PRT_ARCH_NBR IN (45, 46)  "+
"   AND (PRT.PRT_PROC_DTE BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+ 
"        AND PRT.PRT_TRAN_TYP <> 20  "+
"         OR PRT.PRT_PROC_DTE >= TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') - 1  "+
"        AND PRT.PRT_PROC_DTE <= TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') - 1 "+ 
"        AND PRT.PRT_TRAN_TYP = 20) "+ 
" GROUP BY VBA.ENTIDAD_PADRE "+
"           ,PRT.PRT_TRAN_TYP "+
" UNION ALL  "+
" SELECT 2 LIQ  "+
"       ,CASE  "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (94, 95, 96, 97, 98) THEN PE.ENTIDAD_PADRE "+ 
"             ELSE PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
"        END ENTIDAD_PADRE "+
"       ,PTRV.PTRV_TTR_NUMERO TTR  "+
"       ,PTRV.PTRV_IMPORTE_TOTAL_TRANSAC - PTRV.PTRV_IMP_CUOTA_INTERCAMBIO - PTRV.PTRV_IMP_IVA_INTERCAMBIO IMPORTE "+ 
"  FROM PMADMIN.PRSA_TXN_RECHAZADAS_VAL PTRV, "+ 
"       PMADMIN.VW_BUS_ACQ VBA,  "+
"       PMADMIN.PRSA_ENTIDADES PE,  "+
"       PMADMIN.PRSA_TIPOS_TRANSACCION PTT  "+
" WHERE (PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (94, 98, 95, 96, 97)  "+
"         OR PTRV.PTRV_ENT_NUMERO_PROSA_EMI IN (94, 98, 95, 96, 97))  "+
"   AND PTRV.PTRV_FECHA_PROCESO_TRANSAC BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+ 
"   AND PTRV.PTRV_TLI_NUMERO = 2  "+
"   AND PTT.NUMERO = PTRV.PTRV_TTR_NUMERO  "+
"   AND PE.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_EMI  "+
"   AND VBA.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
" UNION ALL "+
" SELECT PP.TLI_NUMERO LIQ "+
"       ,CASE WHEN PE.ENTIDAD_PADRE IN (95, 96, 97) THEN PE2.ENTIDAD_PADRE  "+
"            WHEN PE.ENTIDAD_PADRE = 31 AND PE.NUMERO_PROSA = 131 THEN PE.NUMERO_PROSA "+ 
"            ELSE PE.ENTIDAD_PADRE "+
"        END ENTIDAD_PADRE  "+
"       ,PP.TTR_NUMERO TTR "+ 
"       ,SUM(CASE "+
"                    WHEN PP.TTR_NUMERO IN(11, 16, 19,20,21) THEN -(PP.TOTAL_IMPORTE - PP.IMPORTE_COMISION_INTERCAMB - PP.IVA_COMISION_INTERCAMB) "+ 
"                    ELSE PP.TOTAL_IMPORTE - PP.IMPORTE_COMISION_INTERCAMB - PP.IVA_COMISION_INTERCAMB "+
"                 END) IMPORTE  "+
"  FROM PMADMIN.PRSA_POSICIONES PP,  "+
"       PMADMIN.PRSA_ENTIDADES PE,  "+
"       PMADMIN.PRSA_ENTIDADES PE2,  "+
"       PMADMIN.PRSA_TIPOS_TRANSACCION PTT "+ 
" WHERE PP.TLI_NUMERO = 2  "+
"   AND (PP.TTR_NUMERO IN (10, 11)  "+
"        AND PP.STTR_NUMERO NOT IN (100)  "+
"         OR PP.TTR_NUMERO NOT IN (10, 11))  "+
"   AND (PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+ 
"        AND PP.TTR_NUMERO IN (1, 21, 10, 11, 15, 18, 9, 16, 19)  "+
"         OR PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDateHabAnt"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDateHabAnt"]+"', 'DD/MM/YYYY') "+ 
"        AND PP.TTR_NUMERO IN (20, 24)) "+ 
"   AND PTT.NUMERO = PP.TTR_NUMERO  "+
"   AND PE2.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_ADQ  "+
"   AND PE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI "+ 
" GROUP BY PP.TLI_NUMERO "+
"       ,CASE WHEN PE.ENTIDAD_PADRE IN (95, 96, 97) THEN PE2.ENTIDAD_PADRE  "+
"            WHEN PE.ENTIDAD_PADRE = 31 AND PE.NUMERO_PROSA = 131 THEN PE.NUMERO_PROSA "+ 
"            ELSE PE.ENTIDAD_PADRE "+
"        END  "+
"       ,PP.TTR_NUMERO "+
" UNION ALL "+
//  -- INICIA SALIENTE 
"  SELECT LIQ, ENTIDAD_PADRE, TTR, SUM(IMPORTE) "+
"  FROM ( "+
"  SELECT PP.TLI_NUMERO LIQ  "+
"        ,CASE  "+
"              WHEN PE.ENTIDAD_PADRE IN (95,96,97) THEN PE2.ENTIDAD_PADRE  "+
"              ELSE PE.ENTIDAD_PADRE  "+
"        END ENTIDAD_PADRE "+
"       ,CASE "+
"              WHEN PP.ENT_NUMERO_PROSA_EMI NOT IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_ADQ "+
"              ELSE PP.ENT_NUMERO_PROSA_EMI "+
"        END emi "+
"        ,PP.TTR_NUMERO TTR "+
"        ,ROUND(SUM(  "+
"             CASE  "+
"                  WHEN PP.TTR_NUMERO IN (21,20,16) THEN (PP.TOTAL_IMPORTE-PP.IMPORTE_COMISION_INTERCAMB-PP.IVA_COMISION_INTERCAMB)  "+
"                  ELSE -(PP.TOTAL_IMPORTE-PP.IMPORTE_COMISION_INTERCAMB-PP.IVA_COMISION_INTERCAMB)  "+
"             END),2) IMPORTE  "+
"  FROM PMADMIN.PRSA_POSICIONES PP  "+
"  INNER JOIN PMADMIN.PRSA_ENTIDADES PE  "+
"      ON PE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_ADQ  "+
"  INNER JOIN PMADMIN.PRSA_ENTIDADES PE2  "+
"      ON PE2.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI  "+
"  INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT  "+
"      ON PTT.NUMERO = PP.TTR_NUMERO  "+
"  WHERE PP.TLI_NUMERO = 3  "+
"  AND ((PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (1,21,9,15,16,18))  "+
"      OR  "+
"       (PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (20))  "+
"      )  "+
"  AND PP.TIPO_TARJETA IN ('D','C')  "+
"  GROUP BY PP.TLI_NUMERO  "+
"        ,CASE  "+
"              WHEN PE.ENTIDAD_PADRE IN (95,96,97) THEN PE2.ENTIDAD_PADRE  "+
"              ELSE PE.ENTIDAD_PADRE  "+
"        END  "+
"        ,PE.NUMERO_PROSA "+
"        ,CASE "+
"              WHEN PP.ENT_NUMERO_PROSA_EMI NOT IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_ADQ "+
"              ELSE PP.ENT_NUMERO_PROSA_EMI "+
"        END "+
"        ,PP.TTR_NUMERO  "+
"        ,PP.TIPO_TARJETA "+
"  UNION ALL  "+
"  SELECT 3 LIQ  "+
"        ,DECODE(VBA.NUMERO_LN,'EGLO',PE.ENTIDAD_PADRE,VBA.ENTIDAD_PADRE) ENTIDAD_PADRE "+
"        ,PTRV.PTRV_ENT_NUMERO_PROSA_ADQ EMI "+
"        ,0 TTR  "+
"        ,round(SUM(CASE  "+
"             WHEN PTRV.PTRV_TTR_NUMERO IN (21,20) THEN (PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-PTRV_IMP_CUOTA_INTERCAMBIO-PTRV_IMP_IVA_INTERCAMBIO)  "+
"             ELSE -(PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-PTRV_IMP_CUOTA_INTERCAMBIO-PTRV_IMP_IVA_INTERCAMBIO)  "+
"          END),2) IMPORTE  "+
"  FROM PMADMIN.PRSA_TXN_RECHAZADAS_VAL PTRV  "+
"  INNER JOIN PMADMIN.VW_BUS_ACQ VBA  "+
"      ON VBA.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_ADQ  "+
"  INNER JOIN PMADMIN.PRSA_ENTIDADES PE  "+
"        ON PE.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_EMI  "+
"  WHERE PTRV.PTRV_FECHA_PROCESO_TRANSAC+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY')  "+
"  AND PTRV.PTRV_TTR_NUMERO NOT IN (53)  "+
"  GROUP BY DECODE(VBA.NUMERO_LN,'EGLO',PE.ENTIDAD_PADRE,VBA.ENTIDAD_PADRE) "+
"           ,DECODE(VBA.NUMERO_LN,'EGLO',PE.NUMERO_PROSA,VBA.NUMERO_PROSA) "+
"           ,PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
"           ,PTRV_NATURALEZA_CONTABLE "+
"  UNION ALL  "+
"  SELECT 3 LIQ  "+
"        ,PP.ENT_NUMERO_PROSA_EMI ENTIDAD "+
"        ,PP.ENT_NUMERO_PROSA_ADQ emi "+
"        ,PP.TTR_NUMERO TTR "+
"        ,round(-SUM(PP.TOTAL_IMPORTE),2) IMPORTE   "+
"  FROM PMADMIN.PRSA_POSICIONES PP   "+
"  INNER JOIN PMADMIN.PRSA_ENTIDADES PE   "+
"      ON PE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI   "+
"  INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT   "+
"      ON PTT.NUMERO = PP.TTR_NUMERO   "+
"  WHERE PP.ENT_NUMERO_PROSA_EMI IN (95,96,97)   "+
"  AND PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97)   "+
"  AND ((PP.TTR_NUMERO = 20 AND PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY'))   "+
"      OR (PP.TTR_NUMERO != 20 AND PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY')))  "+
"  GROUP BY PP.ENT_NUMERO_PROSA_EMI "+
"        ,PE.NUMERO_PROSA "+
"        ,PP.ENT_NUMERO_PROSA_ADQ "+
"        ,PP.TTR_NUMERO  "+
"        ,PP.TIPO_TARJETA "+
"  UNION ALL  "+
"  SELECT LIQ, ENTIDAD_PADRE, emi, TTR, ROUND(SUM(IMPORTE),2) IMPORTE "+
"  FROM ( "+
"  SELECT 3 LIQ  "+
"        ,CASE  "+
"              WHEN VISTA.ENTIDAD_PADRE IN (95,96,97) THEN VISTA2.ENTIDAD_PADRE  "+
"              ELSE VISTA.ENTIDAD_PADRE  "+
"         END ENTIDAD_PADRE "+
"        ,VISTA2.NUMERO_PROSA emi "+
"        ,SFIT.FMI_CR_TC_GRP TTR "+
"        ,SUM(CASE "+
" 				WHEN VISTA2.ENTIDAD_PADRE = 31 THEN SFIT.FMI_CHRG_AMT "+
" 				ELSE -SFIT.FMI_CHRG_AMT "+
" 		  END) IMPORTE "+
"  FROM SETTLEMENT.SV_FM_INT_TXN SFIT    "+
"  INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA    "+
"              UNION     "+
"              SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE    "+
"             ) VISTA    "+
"      ON VISTA.BU = SFIT.FMI_CR_CLR_BU    "+
"  INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA    "+
"              UNION     "+
"              SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE    "+
"             ) VISTA2    "+
"      ON VISTA2.BU = SFIT.FMI_DB_CLR_BU    "+
"  WHERE SFIT.FMI_DT+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY')  "+
"  AND FMI_NSI_CLR_ID = 33      "+
"  AND TRIM(FMI_OPER_ID) <> 'CZDCS030'    "+
"  GROUP BY CASE  "+
"              WHEN VISTA.ENTIDAD_PADRE IN (95,96,97) THEN VISTA2.ENTIDAD_PADRE  "+
"              ELSE VISTA.ENTIDAD_PADRE  "+
"         END     "+
"        ,VISTA.NUMERO_PROSA "+
"        ,VISTA2.NUMERO_PROSA "+
"        ,CASE   "+
"              WHEN VISTA.NOMBRE LIKE '%DB%' THEN 'D'   "+
"              ELSE 'C'   "+
"         END "+
"        ,SFIT.FMI_CR_TC_GRP  "+
"  UNION ALL   "+
"  SELECT 3 LIQ   "+
"        ,VISTA.ENTIDAD_PADRE ENTIDAD_PADRE "+
"        ,VISTA2.NUMERO_PROSA emi "+
"        ,PTS.PTS_TXN_TYP TTR  "+
"        ,-SUM(-PTS.PTS_CR_TXN_AMT+PTS.PTS_DB_TXN_AMT) IMPORTE   "+
"  FROM PMADMIN.PRSA_TXN_SUMMARY PTS   "+
"  INNER JOIN PMADMIN.PRSA_TXN_SUMMARY PTS2   "+
"      ON PTS.PTS_CYCLE_ID = PTS2.PTS_CYCLE_ID      "+
"      AND PTS.PTS_CYCLE_DT = PTS2.PTS_CYCLE_DT      "+
"      AND PTS.PTS_NSI = PTS2.PTS_NSI      "+
"      AND PTS.PTS_NSI_CLR_ID = PTS2.PTS_NSI_CLR_ID      "+
"      AND PTS.PTS_SRCE_C = PTS2.PTS_SRCE_C      "+
"      AND PTS.PTS_PROC_CD = PTS2.PTS_PROC_CD      "+
"      AND PTS.PTS_TXN_TYP = PTS2.PTS_TXN_TYP      "+
"      AND PTS.PTS_FROM_ACCT = PTS2.PTS_FROM_ACCT      "+
"      AND PTS.PTS_TO_ACCT = PTS2.PTS_TO_ACCT      "+
"      AND PTS.PTS_TXN_CD_SFX = PTS2.PTS_TXN_CD_SFX      "+
"      AND PTS.PTS_CR_CNT = PTS2.PTS_DB_CNT      "+
"      AND PTS.PTS_CR_TXN_AMT = PTS2.PTS_DB_TXN_AMT      "+
"      AND PTS.PTS_CR_MRCH_FEE_AMT = PTS2.PTS_DB_MRCH_FEE_AMT      "+
"      AND PTS.PTS_CR_MRCH_IVA_AMT = PTS2.PTS_DB_MRCH_IVA_AMT      "+
"      AND PTS.PTS_CR_INTCHG_FEE_AMT = PTS2.PTS_DB_INTCHG_FEE_AMT      "+
"      AND PTS.PTS_CR_INTCHG_IVA_AMT = PTS2.PTS_DB_INTCHG_IVA_AMT      "+
"      AND PTS.PTS_DB_CNT = PTS2.PTS_CR_CNT      "+
"      AND PTS.PTS_DB_TXN_AMT = PTS2.PTS_CR_TXN_AMT      "+
"      AND PTS.PTS_DB_MRCH_FEE_AMT = PTS2.PTS_CR_MRCH_FEE_AMT      "+
"      AND PTS.PTS_DB_MRCH_IVA_AMT = PTS2.PTS_CR_MRCH_IVA_AMT      "+
"      AND PTS.PTS_DB_INTCHG_FEE_AMT = PTS2.PTS_CR_INTCHG_FEE_AMT      "+
"      AND PTS.PTS_DB_INTCHG_IVA_AMT = PTS2.PTS_CR_INTCHG_IVA_AMT      "+
"  INNER JOIN PMADMIN.SZ_TC_GRP_RELATION STGR   "+
"      ON STGR.TCGR_PROC_CD = PTS.PTS_PROC_CD   "+
"      AND STGR.TCGR_SRCE_C = PTS.PTS_SRCE_C   "+
"      AND STGR.TCGR_TO_ACCT = PTS.PTS_TO_ACCT   "+
"      AND STGR.TCGR_TXN_CD_SFX = PTS.PTS_TXN_CD_SFX   "+
"      AND STGR.TCGR_TXN_TYP = PTS.PTS_TXN_TYP   "+
"  INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA   "+
"              UNION    "+
"              SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE   "+
"             ) VISTA   "+
