"   GROUP BY LIQ, ENTIDAD_PADRE, EMI, TTR        "+
" )   "+
"  GROUP BY LIQ, ENTIDAD_PADRE, TTR        "+
//  -- TERMINA SALIENTE
" ) DATOS "+
" GROUP BY DATOS.ENTIDAD "+
" ORDER BY 1 "+
" ) DATOS2 "+
" WHERE DATOS2.ENTIDAD_PADRE NOT IN (94,95,96,97,98) "+
" GROUP BY DATOS2.ENTIDAD_PADRE "+
// SUPER UNION 
" UNION ALL "+
" SELECT TABLA.ENTIDAD "+
"       ,(SELECT PE.DESCRIPCION FROM PMADMIN.PRSA_ENTIDADES PE WHERE PE.NUMERO_PROSA = TABLA.ENTIDAD) DESC_ENTIDAD "+
"       ,SUM(CASE "+
"             WHEN TABLA.LIQ = 2 THEN -TABLA.IMP "+
"             ELSE 0 "+
"        END) ENTRANTE "+
"       ,SUM(CASE "+
"             WHEN TABLA.LIQ = 3 THEN TABLA.IMP "+
"             ELSE 0 "+
"        END) SALIENTE "+      
" FROM ( "+
" SELECT PP.TLI_NUMERO LIQ "+
"       ,CASE "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_ADQ "+
"             ELSE PP.ENT_NUMERO_PROSA_EMI "+
"        END ENTIDAD "+
"       ,PP.TTR_NUMERO TTR "+
"       ,SUM(CASE "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN -PP.TOTAL_IMPORTE "+
"             ELSE PP.TOTAL_IMPORTE "+
"        END) IMP "+
" FROM PMADMIN.PRSA_POSICIONES PP "+
" WHERE ((PP.TLI_NUMERO = 3  "+
"         AND PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+ 
"         AND PP.TTR_NUMERO IN (1,21,20,15,9,18,16) "+
"        ) "+
"        OR "+
"        (PP.TLI_NUMERO = 2 "+ 
"         AND ( "+
"              (PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (1,21,9,15,18,16,19)) "+ 
"              OR "+
"              (PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (20,24)) "+ 
"             ) "+
"        ) "+
"       ) "+
" AND (PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) OR PP.ENT_NUMERO_PROSA_EMI IN (95,96,97)) "+
" GROUP BY PP.TLI_NUMERO "+
"       ,CASE "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_ADQ "+
"             ELSE PP.ENT_NUMERO_PROSA_EMI "+
"        END "+
"       ,PP.TTR_NUMERO "+
" UNION ALL "+
" SELECT PP.TLI_NUMERO LIQ "+
"       ,CASE "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_ADQ "+
"             ELSE PP.ENT_NUMERO_PROSA_EMI "+
"        END ENTIDAD "+
"       ,0 TTR "+
"       ,SUM(CASE "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN PP.IMPORTE_COMISION_INTERCAMB "+
"             ELSE -PP.IMPORTE_COMISION_INTERCAMB "+
"        END) IMP "+
" FROM PMADMIN.PRSA_POSICIONES PP "+
" INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT "+
"     ON PTT.NUMERO = PP.TTR_NUMERO "+
" WHERE ((PP.TLI_NUMERO = 3  "+
"         AND PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+ 
"         AND PP.TTR_NUMERO IN (1,21,20,15,9,18,16) "+
"        ) "+
"        OR "+
"        (PP.TLI_NUMERO = 2 "+ 
"         AND ( "+
"              (PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (1,21,9,15,18,16,19)) "+ 
"              OR "+
"              (PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (20,24)) "+ 
"             ) "+
"        ) "+
"       ) "+
" AND (PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) OR PP.ENT_NUMERO_PROSA_EMI IN (95,96,97)) "+
" GROUP BY PP.TLI_NUMERO "+
"       ,CASE "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_ADQ "+
"             ELSE PP.ENT_NUMERO_PROSA_EMI "+
"        END "+
" UNION ALL "+
" SELECT PP.TLI_NUMERO LIQ "+
"       ,CASE "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_ADQ "+
"             ELSE PP.ENT_NUMERO_PROSA_EMI "+
"        END ENTIDAD "+
"       ,99 TTR "+
"       ,SUM(CASE "+
"             WHEN PP.TTR_NUMERO IN (20,21) THEN PP.IVA_COMISION_INTERCAMB "+
"             ELSE -PP.IVA_COMISION_INTERCAMB "+
"        END) IMP "+
" FROM PMADMIN.PRSA_POSICIONES PP "+
" INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT "+
"     ON PTT.NUMERO = PP.TTR_NUMERO "+
" WHERE ((PP.TLI_NUMERO = 3  "+
"         AND PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+ 
"         AND PP.TTR_NUMERO IN (1,21,20,15,9,18,16) "+
"        ) "+
"        OR "+
"        (PP.TLI_NUMERO = 2 "+ 
"         AND ( "+
"              (PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (1,21,9,15,18,16,19)) "+ 
"              OR "+
"              (PP.FECHA_PROCESO+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (20,24)) "+ 
"             ) "+
"        ) "+
"       ) "+
" AND (PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) OR PP.ENT_NUMERO_PROSA_EMI IN (95,96,97)) "+
" GROUP BY PP.TLI_NUMERO "+
"       ,CASE "+
"             WHEN PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_ADQ "+
"             ELSE PP.ENT_NUMERO_PROSA_EMI "+
"        END "+
" UNION ALL "+
" SELECT 2 LIQ "+
"       ,PRT.PRT_PROSA_ISS_ENTITY ENTIDAD "+
"       ,69 TTR "+
"       ,SUM(CASE "+
"             WHEN PRT.PRT_TRAN_TYP = 21 THEN -PRT.PRT_TRAN_AMT "+
"             ELSE PRT.PRT_TRAN_AMT "+
"         END) IMP "+
" FROM PMADMIN.PRSA_REJECTED_TXN PRT "+
" WHERE PRT.PRT_ARCH_NBR IN (45,46)  "+
" AND PRT.PRT_TRAN_TYP NOT IN (51,52,53)  "+
" AND (PRT.PRT_PROC_DTE BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+  
"         AND PRT.PRT_TRAN_TYP NOT IN (20, 24)  "+
"          OR PRT.PRT_PROC_DTE >= TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') - 1   "+
"         AND PRT.PRT_PROC_DTE <= TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') - 1 "+  
"         AND PRT.PRT_TRAN_TYP IN (20, 24)) "+
"         AND PRT.PRT_PROSA_ISS_ENTITY IN (95,96,97) "+
