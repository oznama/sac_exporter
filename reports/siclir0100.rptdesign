"       ,SUM(CASE "+
"             WHEN PRT.PRT_TRAN_TYP = 21 THEN PRT.PRT_INTCHG_FEE_AMT "+
"             ELSE -PRT.PRT_INTCHG_FEE_AMT "+
"         END) IMP "+
" FROM PMADMIN.PRSA_REJECTED_TXN PRT "+
" WHERE PRT.PRT_ARCH_NBR IN (45,46)  "+
" AND PRT.PRT_TRAN_TYP NOT IN (51,52,53)  "+
" AND (PRT.PRT_PROC_DTE BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+  
"         AND PRT.PRT_TRAN_TYP NOT IN (20, 24)  "+
"          OR PRT.PRT_PROC_DTE >= TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') - 1   "+
"         AND PRT.PRT_PROC_DTE <= TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') - 1 "+  
"         AND PRT.PRT_TRAN_TYP IN (20, 24)) "+
"         AND PRT.PRT_PROSA_ISS_ENTITY IN (95,96,97) "+
" GROUP BY PRT.PRT_PROSA_ISS_ENTITY "+
" UNION ALL "+
" SELECT 2 LIQ "+
"       ,PRT.PRT_PROSA_ISS_ENTITY ENTIDAD "+
"       ,69 TTR "+
"       ,SUM(CASE "+
"             WHEN PRT.PRT_TRAN_TYP = 21 THEN PRT.PRT_INTCHG_IVA_AMT "+
"             ELSE -PRT.PRT_INTCHG_IVA_AMT "+
"         END) IMP "+
" FROM PMADMIN.PRSA_REJECTED_TXN PRT "+
" WHERE PRT.PRT_ARCH_NBR IN (45,46)  "+
" AND PRT.PRT_TRAN_TYP NOT IN (51,52,53)  "+
" AND (PRT.PRT_PROC_DTE BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+  
"         AND PRT.PRT_TRAN_TYP NOT IN (20, 24)  "+
"          OR PRT.PRT_PROC_DTE >= TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') - 1   "+
"         AND PRT.PRT_PROC_DTE <= TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') - 1 "+  
"         AND PRT.PRT_TRAN_TYP IN (20, 24)) "+
"         AND PRT.PRT_PROSA_ISS_ENTITY IN (95,96,97) "+
" GROUP BY PRT.PRT_PROSA_ISS_ENTITY "+
" UNION ALL "+
" SELECT 3 LIQ "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
"             ELSE PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
"        END ENTIDAD "+
"        ,69 TTR "+
"       ,SUM(PTRV.PTRV_IMPORTE_TOTAL_TRANSAC) IMP "+
" FROM PMADMIN.PRSA_TXN_RECHAZADAS_VAL PTRV "+
" WHERE PTRV.PTRV_FECHA_PROCESO_TRANSAC+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+ 
" AND PTRV.PTRV_TTR_NUMERO NOT IN (53) "+
" AND (PTRV.PTRV_ENT_NUMERO_PROSA_ADQ NOT IN (94,95,96,97,98) OR PTRV.PTRV_ENT_NUMERO_PROSA_EMI NOT IN (94,95,96,97,98)) "+
" GROUP BY CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
"             ELSE PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
"        END "+
" UNION ALL "+
" SELECT 2 LIQ "+
"       ,CASE "+
"             WHEN PFM.PFM_ENT_NUMERO_PROSA IN (95,96,97) THEN PFM.PFM_ENT_NUMERO_PROSA "+
"             ELSE PFM.PFM_ENT_NUMERO_PROSA_ASOC "+
"        END ENTIDAD "+
"       ,PFM.PFM_TMI_NUMERO TTR "+
"       ,SUM(CASE "+
"             WHEN PFM.PFM_TTR_NUMERO = 10 THEN PFM.PFM_IMPORTE_ORIGEN "+
"             ELSE -PFM.PFM_IMPORTE_ORIGEN "+
"       END) IMP "+
" FROM PMADMIN.PRSA_F_MISCELANEOS PFM "+
" WHERE PFM.PFM_FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+
" AND PFM.PFM_TMI_NUMERO BETWEEN 102 AND 112 "+
" AND PFM.PFM_ARCH_NUMERO IN (45,46) "+
" AND (PFM.PFM_ENT_NUMERO_PROSA IN (95,96,97) OR PFM.PFM_ENT_NUMERO_PROSA_ASOC IN (95,96,97)) "+ 
" GROUP BY CASE "+
"             WHEN PFM.PFM_ENT_NUMERO_PROSA IN (95,96,97) THEN PFM.PFM_ENT_NUMERO_PROSA "+
"             ELSE PFM.PFM_ENT_NUMERO_PROSA_ASOC "+
"          END "+
"         ,PFM.PFM_TMI_NUMERO "+
" UNION ALL "+
" SELECT DATOS.LIQ "+
"       ,DATOS.ENTIDAD "+
"       ,DATOS.TTR "+
"       ,SUM(DATOS.IMP) IMP "+
" FROM ( "+
" SELECT 3 LIQ "+
"       ,VE.NUMERO_PROSA ENTIDAD "+
"       ,CASE "+
"             WHEN SFIT.FMI_OPER_ID = 'CZDCS035        ' THEN 103 "+
"             WHEN SFIT.FMI_OPER_ID = 'CZDCS025_112    ' THEN 112 "+
"             ELSE 199 "+
"        END TTR "+
"       ,SUM(SFIT.FMI_CHRG_AMT) IMP "+
" FROM SETTLEMENT.SV_FM_INT_TXN SFIT,  "+
" (SELECT * FROM PMADMIN.VW_BUS_EMI UNION SELECT * FROM PMADMIN.VW_BUS_ACQ  ) VE,  "+
" (SELECT * FROM PMADMIN.VW_BUS_ACQ UNION SELECT * FROM PMADMIN.VW_BUS_EMI ) VA "+
" WHERE FMI_DT+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+ 
" AND SFIT.FMI_DB_CLR_BU = VE.BU_TX_ISS  "+
" AND SFIT.FMI_CR_CLR_BU = VA.BU_TX_ADQ "+ 
" AND SFIT.FMI_NSI_CLR_ID = 33  "+
" AND TRIM(FMI_OPER_ID)<>'CZDCS030' "+
" AND VE.NUMERO_PROSA IN (95,96,97) "+
" GROUP BY VE.NUMERO_PROSA "+
"       ,CASE "+
"             WHEN SFIT.FMI_OPER_ID = 'CZDCS035        ' THEN 103 "+
"             WHEN SFIT.FMI_OPER_ID = 'CZDCS025_112    ' THEN 112 "+
"             ELSE 199 "+
"        END "+
" UNION ALL "+
" SELECT 3 LIQ "+
"       ,VW.NUMERO_PROSA ENTIDAD    "+
"       ,TO_NUMBER(1||PTS1.PTS_TXN_CD_SFX) TTR "+    
"       ,SUM(CASE "+
"             WHEN STGR.TCGR_GRP_ID = 112 THEN DECODE(PTS1.PTS_DB_TXN_AMT,0,PTS1.PTS_CR_TXN_AMT, PTS1.PTS_DB_TXN_AMT) "+
"             ELSE -DECODE(PTS1.PTS_DB_TXN_AMT,0,PTS1.PTS_CR_TXN_AMT, PTS1.PTS_DB_TXN_AMT) "+
"        END) IMPORTE    "+
"  FROM PMADMIN.PRSA_TXN_SUMMARY PTS1 "+   
