"             ON PTS1.PTS_SRCE_C = STGR.TCGR_SRCE_C "+
"             AND PTS1.PTS_PROC_CD = STGR.TCGR_PROC_CD "+
"             AND PTS1.PTS_TXN_TYP = STGR.TCGR_TXN_TYP "+
"             AND PTS1.PTS_FROM_ACCT = STGR.TCGR_FROM_ACCT "+
"             AND PTS1.PTS_TO_ACCT = STGR.TCGR_TO_ACCT "+
"             AND PTS1.PTS_TXN_CD_SFX = STGR.TCGR_TXN_CD_SFX "+
"        INNER JOIN SETTLEMENT.SZ_TC_GRP STG "+
"          ON STGR.TCGR_GRP_ID  = STG.TCG_GRP_ID "+   
"        INNER JOIN CORE.CD_TXN_CD  CTC  "+
"               ON PTS1.PTS_SRCE_C = CTC.TCX_SRCE_C    "+
"             AND PTS1.PTS_PROC_CD = CTC.TCX_PROC_CD    "+
"             AND PTS1.PTS_TXN_TYP = CTC.TCX_TXN_TYP    "+
"             AND PTS1.PTS_FROM_ACCT = CTC.TCX_FROM_ACCT "+   
"             AND PTS1.PTS_TO_ACCT = CTC.TCX_TO_ACCT    "+
"             AND PTS1.PTS_TXN_CD_SFX = CTC.TCX_TXN_CD_SFX "+   
"        INNER JOIN SETTLEMENT.SV_NSI_CLR_ID  SNCI  "+
"               ON PTS1.PTS_NSI_CLR_ID = SNCI.NCI_NSI_CLR_ID    "+
"  WHERE PTS1.PTS_PROC_DT+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD-MM-YYYY') AND  TO_DATE('"+params["endDate"]+"','DD-MM-YYYY') "+ 
"  AND PTS1.PTS_NSI_CLR_ID IN (33) "+   
"  AND BU_TX_PARENT = 93   "+
"  AND (PTS1.PTS_TXN_TYP NOT IN ('22') OR PTS1.PTS_TXN_CD_SFX NOT IN ('00')) "+
"  AND STGR.TCGR_GRP_ID IN (111,112) "+
"  AND VW.NUMERO_PROSA IN (95,96,97) "+   
