" AND PTRV.PTRV_TTR_NUMERO NOT IN (53) "+
" AND (PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) OR PTRV.PTRV_ENT_NUMERO_PROSA_EMI IN (95,96,97)) "+
" GROUP BY "+
