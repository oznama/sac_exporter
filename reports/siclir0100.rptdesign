"             ) VISTA2   "+
"      ON VISTA2.BU = PTS2.PTS_CLR_BU   "+
"  WHERE PTS.PTS_NSI_CLR_ID = 33   "+
"  AND STGR.TCGR_GRP_ID IN (111,112)   "+
"  AND PTS.PTS_CYCLE_DT+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY')  "+
"  GROUP BY VISTA.ENTIDAD_PADRE   "+
"        ,VISTA.NUMERO_PROSA "+
"        ,VISTA2.NUMERO_PROSA "+
"        ,PTS.PTS_TXN_TYP "+
"        ,CASE  "+
"              WHEN VISTA.NOMBRE LIKE 'DB' THEN 'D'  "+
"              ELSE 'C'  "+
"        END "+
