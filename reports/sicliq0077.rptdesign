            <method name="beforeOpen"><![CDATA[this.queryText=
" SELECT ENTIDAD_PADRE, DESCRIPCION, BU, DESCRIPCION_TTX, ADQ, TIPO_TARJETA, SUM(TXN) TXN, ROUND(SUM(IMPORTE),2) IMPORTE  "+
" FROM ( "+
"  SELECT   "+
"        CASE  "+
"              WHEN PE.ENTIDAD_PADRE IN (95,96,97) THEN PE2.ENTIDAD_PADRE  "+
"              ELSE PE.ENTIDAD_PADRE  "+
"        END ENTIDAD_PADRE   "+
"        ,CASE  "+
"              WHEN PE.ENTIDAD_PADRE IN (95,96,97) THEN PE2.DESCRIPCION  "+
"              ELSE PE.DESCRIPCION  "+
"        END DESCRIPCION   "+
"        ,CASE  "+
"              WHEN PE.ENTIDAD_PADRE IN (95,96,97) THEN  (SELECT MAX(VBA.BU_TX_PARENT) FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.ENTIDAD_PADRE = PE2.ENTIDAD_PADRE)  "+
"              ELSE (SELECT MAX(VBA.BU_TX_PARENT) FROM PMADMIN.VW_BUS_ACQ VBA WHERE VBA.ENTIDAD_PADRE = PE.ENTIDAD_PADRE)  "+
"        END BU  "+
"        ,PTT.DESCRIPCION DESCRIPCION_TTX "+
"        ,pe2.numero_prosa adq "+
"        ,PP.TIPO_TARJETA TIPO_TARJETA  "+
"        ,SUM(PP.NUMERO_TRANSACCIONES) TXN  "+
"        ,SUM(  "+
" 			CASE  "+
" 				 WHEN PP.TTR_NUMERO IN (16,19) THEN -(PP.TOTAL_IMPORTE-PP.IMPORTE_COMISION_INTERCAMB-PP.IVA_COMISION_INTERCAMB)  "+
" 				 ELSE PP.TOTAL_IMPORTE-PP.IMPORTE_COMISION_INTERCAMB-PP.IVA_COMISION_INTERCAMB  "+
" 			END) IMPORTE  "+
"  FROM RLQ.TBL_POSICIONES_LIQ@LG_PMTU_IFO_01 PP  "+
"  INNER JOIN PMADMIN.PRSA_ENTIDADES PE  "+
"      ON PE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_ADQ  "+
"  INNER JOIN PMADMIN.PRSA_ENTIDADES PE2  "+
"      ON PE2.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI  "+
"  INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT  "+
"      ON PTT.NUMERO = PP.TTR_NUMERO  "+
"  WHERE ((PP.FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (1,21,9,15,16,18,19))  "+
"      OR  "+
"       (PP.FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO IN (20))  "+
"      )  "+
"  AND PP.TLI_NUMERO = 3  "+
"  AND PP.TIPO_TARJETA IN ('D','C')  "+
"  GROUP BY    "+
"        CASE  "+
"              WHEN PE.ENTIDAD_PADRE IN (95,96,97) THEN PE2.ENTIDAD_PADRE  "+
"              ELSE PE.ENTIDAD_PADRE  "+
"        END   "+
"        ,CASE  "+
"              WHEN PE.ENTIDAD_PADRE IN (95,96,97) THEN PE2.DESCRIPCION  "+
"              ELSE PE.DESCRIPCION  "+
"        END  "+
"        ,PE.ENTIDAD_PADRE  "+
"        ,PE2.ENTIDAD_PADRE  "+
"        ,PTT.DESCRIPCION   "+
"        ,pe2.numero_prosa "+
"        ,PP.TIPO_TARJETA  "+
"  UNION ALL  "+
"  SELECT   "+
"   DECODE(VBA.NUMERO_LN,'EGLO',PE.ENTIDAD_PADRE,VBA.ENTIDAD_PADRE) ENTIDAD_PADRE  "+
"  ,DECODE(VBA.NUMERO_LN,'EGLO',PE.DESCRIPCION,VBA.DESCRIPCION) DESCRIPCION  "+
"        ,0 BU  "+
"        ,'RS' DESCRIPCION_TTX "+
"        ,vba.numero_prosa adq "+
"        ,PTRV.PTRV_NATURALEZA_CONTABLE TIPO_TARJETA  "+
"        ,COUNT(1) TXN  "+
"        ,SUM(CASE  "+
" 			WHEN PTRV.PTRV_TTR_NUMERO IN (21) THEN -(PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-PTRV_IMP_CUOTA_INTERCAMBIO-PTRV_IMP_IVA_INTERCAMBIO)  "+
" 			ELSE (PTRV.PTRV_IMPORTE_TOTAL_TRANSAC-PTRV_IMP_CUOTA_INTERCAMBIO-PTRV_IMP_IVA_INTERCAMBIO)  "+
"  		END) IMPORTE  "+
"  FROM PMADMIN.PRSA_TXN_RECHAZADAS_VAL PTRV  "+
"  INNER JOIN PMADMIN.VW_BUS_ACQ VBA  "+
"      ON VBA.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_ADQ  "+
"  INNER JOIN PMADMIN.PRSA_ENTIDADES PE  "+
"  	  ON PE.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_EMI  "+
"  WHERE PTRV.PTRV_FECHA_PROCESO_TRANSAC BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY')-1 AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY')-1  "+
"  AND PTRV.PTRV_TTR_NUMERO NOT IN (53)  "+
"  GROUP BY   "+
"   DECODE(VBA.NUMERO_LN,'EGLO',PE.ENTIDAD_PADRE,VBA.ENTIDAD_PADRE)  "+
"  ,DECODE(VBA.NUMERO_LN,'EGLO',PE.DESCRIPCION,VBA.DESCRIPCION) "+
"  ,vba.numero_prosa "+
"        ,PTRV.PTRV_NATURALEZA_CONTABLE  "+
"  UNION ALL  "+
"  SELECT    "+
"         PP.ENT_NUMERO_PROSA_EMI ENTIDAD   "+
"        ,PE.DESCRIPCION DESCRIPCION   "+
"        ,0 BU   "+
"        ,PTT.DESCRIPCION TXN  "+
"        ,PP.ENT_NUMERO_PROSA_ADQ ADQ "+
"        ,PP.TIPO_TARJETA TIPO_TARJETA  "+
"        ,COUNT(1) TXN   "+
"        ,SUM(PP.TOTAL_IMPORTE) IMPORTE   "+
"  FROM RLQ.TBL_POSICIONES_LIQ@LG_PMTU_IFO_01 PP   "+
"  INNER JOIN PMADMIN.PRSA_ENTIDADES PE   "+
"      ON PE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI   "+
"  INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT   "+
"      ON PTT.NUMERO = PP.TTR_NUMERO   "+
"  WHERE ((PP.FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO = 20)   "+
"      OR (PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') AND PP.TTR_NUMERO != 20))  "+
"  AND PP.ENT_NUMERO_PROSA_EMI IN (95,96,97)   "+
"  AND PP.ENT_NUMERO_PROSA_ADQ IN (95,96,97)   "+
"  GROUP BY    "+
"         PP.ENT_NUMERO_PROSA_EMI   "+
"        ,PE.DESCRIPCION   "+
"        ,PTT.DESCRIPCION "+
"        ,PP.ENT_NUMERO_PROSA_ADQ "+
"        ,PP.TIPO_TARJETA  "+
"  UNION ALL  "+
"  SELECT     "+
"         CASE  "+
"              WHEN VISTA.ENTIDAD_PADRE IN (95,96,97) THEN VISTA2.ENTIDAD_PADRE  "+
"              ELSE VISTA.ENTIDAD_PADRE  "+
"         END ENTIDAD_PADRE     "+
"        ,CASE  "+
"              WHEN VISTA.ENTIDAD_PADRE IN (95,96,97) THEN VISTA2.DESCRIPCION  "+
"              ELSE VISTA.DESCRIPCION  "+
"         END DESCRIPCION     "+
"        ,CASE  "+
"              WHEN VISTA.ENTIDAD_PADRE IN (95,96,97) THEN VISTA2.BU_TX_PARENT  "+
"              ELSE VISTA.BU_TX_PARENT  "+
"         END BU  "+
"        ,CASE    "+
"              WHEN VISTA.BU_TX_PARENT = 93 THEN 'MISCELANEOS (EN CONTRA)'    "+
"              ELSE 'MISCELANEOS (A FAVOR)'    "+
"        END DESCRIPCION_TTX    "+
"        ,VISTA2.NUMERO_PROSA ADQ "+
"        ,CASE    "+
"              WHEN VISTA.NOMBRE LIKE '%DB%' THEN 'D'    "+
"              ELSE 'C'    "+
"        END TIPO_TARJETA    "+
"        ,COUNT(1) TXN    "+
"        ,SUM(SFIT.FMI_CHRG_AMT) IMPORTE  "+
"  FROM SETTLEMENT.SV_FM_INT_TXN SFIT    "+
"  INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA    "+
"              UNION     "+
"              SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE    "+
"             ) VISTA    "+
"      ON VISTA.BU = SFIT.FMI_CR_CLR_BU    "+
"  INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA    "+
"              UNION     "+
"              SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE    "+
"             ) VISTA2    "+
"      ON VISTA2.BU = SFIT.FMI_DB_CLR_BU    "+
"  WHERE SFIT.FMI_DT BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY')-1 AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY')-1  "+
"  AND FMI_NSI_CLR_ID = 33      "+
"  AND TRIM(FMI_OPER_ID) <> 'CZDCS030'    "+
"  GROUP BY    "+
"         VISTA.ENTIDAD_PADRE    "+
"        ,VISTA.DESCRIPCION    "+
"        ,VISTA.BU_TX_PARENT  "+
"        ,VISTA2.ENTIDAD_PADRE    "+
"        ,VISTA2.DESCRIPCION    "+
"        ,VISTA2.BU_TX_PARENT      "+
"        ,CASE    "+
"              WHEN VISTA.BU_TX_PARENT = 93 THEN 'MISCELANEOS (EN CONTRA)'    "+
"              ELSE 'MISCELANEOS (A FAVOR)'    "+
"        END "+
"        ,VISTA2.NUMERO_PROSA "+
"        ,CASE    "+
"              WHEN VISTA.NOMBRE LIKE '%DB%' THEN 'D'    "+
"              ELSE 'C'    "+
"        END  "+
"  UNION ALL   "+
"  SELECT    "+
"         VISTA.ENTIDAD_PADRE ENTIDAD_PADRE   "+
"        ,VISTA.DESCRIPCION DESCRIPCION   "+
"        ,VISTA.BU BU   "+
"        ,CASE   "+
"              WHEN PTS.PTS_CR_CNT >0 THEN 'MISCELANEOS (EN CONTRA)'   "+
"              ELSE 'MISCELANEOS (A FAVOR)'   "+
"        END DESCRIPCION_TTX "+
"        ,VISTA2.NUMERO_PROSA ADQ "+
"        ,CASE   "+
"              WHEN VISTA.NOMBRE LIKE 'DB' THEN 'D'   "+
"              ELSE 'C'   "+
"        END TIPO_TARJETA   "+
"        ,SUM(PTS.PTS_CR_CNT+PTS.PTS_DB_CNT) TXN   "+
"        ,SUM(PTS.PTS_CR_TXN_AMT+PTS.PTS_DB_TXN_AMT) IMPORTE   "+
"  FROM PMADMIN.PRSA_TXN_SUMMARY PTS   "+
"  INNER JOIN PMADMIN.PRSA_TXN_SUMMARY PTS2   "+
"      ON PTS.PTS_CYCLE_ID = PTS2.PTS_CYCLE_ID      "+
"      AND PTS.PTS_CYCLE_DT = PTS2.PTS_CYCLE_DT      "+
"      AND PTS.PTS_NSI = PTS2.PTS_NSI      "+
"      AND PTS.PTS_NSI_CLR_ID = PTS2.PTS_NSI_CLR_ID      "+
"      AND PTS.PTS_SRCE_C = PTS2.PTS_SRCE_C      "+
"      AND PTS.PTS_PROC_CD = PTS2.PTS_PROC_CD      "+
"      AND PTS.PTS_TXN_TYP = PTS2.PTS_TXN_TYP      "+
"      AND PTS.PTS_FROM_ACCT = PTS2.PTS_FROM_ACCT      "+
"      AND PTS.PTS_TO_ACCT = PTS2.PTS_TO_ACCT      "+
"      AND PTS.PTS_TXN_CD_SFX = PTS2.PTS_TXN_CD_SFX      "+
"      AND PTS.PTS_CR_CNT = PTS2.PTS_DB_CNT      "+
"      AND PTS.PTS_CR_TXN_AMT = PTS2.PTS_DB_TXN_AMT      "+
"      AND PTS.PTS_CR_MRCH_FEE_AMT = PTS2.PTS_DB_MRCH_FEE_AMT      "+
"      AND PTS.PTS_CR_MRCH_IVA_AMT = PTS2.PTS_DB_MRCH_IVA_AMT      "+
"      AND PTS.PTS_CR_INTCHG_FEE_AMT = PTS2.PTS_DB_INTCHG_FEE_AMT      "+
"      AND PTS.PTS_CR_INTCHG_IVA_AMT = PTS2.PTS_DB_INTCHG_IVA_AMT      "+
"      AND PTS.PTS_DB_CNT = PTS2.PTS_CR_CNT      "+
"      AND PTS.PTS_DB_TXN_AMT = PTS2.PTS_CR_TXN_AMT      "+
"      AND PTS.PTS_DB_MRCH_FEE_AMT = PTS2.PTS_CR_MRCH_FEE_AMT      "+
"      AND PTS.PTS_DB_MRCH_IVA_AMT = PTS2.PTS_CR_MRCH_IVA_AMT      "+
"      AND PTS.PTS_DB_INTCHG_FEE_AMT = PTS2.PTS_CR_INTCHG_FEE_AMT      "+
"      AND PTS.PTS_DB_INTCHG_IVA_AMT = PTS2.PTS_CR_INTCHG_IVA_AMT      "+
"  INNER JOIN PMADMIN.SZ_TC_GRP_RELATION STGR   "+
"      ON STGR.TCGR_PROC_CD = PTS.PTS_PROC_CD   "+
"      AND STGR.TCGR_SRCE_C = PTS.PTS_SRCE_C   "+
"      AND STGR.TCGR_TO_ACCT = PTS.PTS_TO_ACCT   "+
"      AND STGR.TCGR_TXN_CD_SFX = PTS.PTS_TXN_CD_SFX   "+
"      AND STGR.TCGR_TXN_TYP = PTS.PTS_TXN_TYP   "+
"  INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA   "+
"              UNION    "+
"              SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE   "+
"             ) VISTA   "+
"      ON VISTA.BU = PTS.PTS_CLR_BU  "+
"      AND VISTA.ENTIDAD_PADRE NOT IN (95,96,97)   "+
"  INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA   "+
"              UNION    "+
"              SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE   "+
"             ) VISTA2   "+
"      ON VISTA2.BU = PTS2.PTS_CLR_BU   "+
"  WHERE PTS.PTS_CYCLE_DT BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY')-1 AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY')-1  "+
"  AND PTS.PTS_NSI_CLR_ID = 33   "+
"  AND STGR.TCGR_GRP_ID IN (111,112)   "+
"  GROUP BY    "+
"         VISTA.ENTIDAD_PADRE   "+
"        ,VISTA.DESCRIPCION   "+
"        ,VISTA.BU   "+
"        ,CASE   "+
"              WHEN PTS.PTS_CR_CNT >0 THEN 'MISCELANEOS (EN CONTRA)'   "+
"              ELSE 'MISCELANEOS (A FAVOR)'   "+
"        END   "+
"        ,VISTA2.NUMERO_PROSA "+
"        ,CASE   "+
"              WHEN VISTA.NOMBRE LIKE 'DB' THEN 'D'   "+
"              ELSE 'C'   "+
"        END  "+
" )        "+
" GROUP BY ENTIDAD_PADRE, DESCRIPCION, BU, DESCRIPCION_TTX, ADQ, TIPO_TARJETA"+
" ORDER BY DESCRIPCION_TTX, ADQ, TIPO_TARJETA ";]]></method>
