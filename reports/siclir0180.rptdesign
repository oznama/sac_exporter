                </structure>
            </list-property>
            <property name="queryText">SELECT  ent.ENT_NUMERO_PROSA
       /* Inicia  cambio: AXIA-AVC-C-24-5065-20 13/05/2021 */
       /* Inicia  cambio: AXIA-GGB-C-24-5065-20 12/08/2020 */
       ,ent2.NOMBRE
       ,SUM(DECODE(sal.TLI_NUMERO, 6, DECODE(SIGN(TOTAL_IMPORTE), 1,TOTAL_IMPORTE,0), 0))  POS
       ,SUM(DECODE(sal.TLI_NUMERO, 6, DECODE(SIGN(TOTAL_IMPORTE),-1,TOTAL_IMPORTE,0), 0))
--      + SUM(DECODE(sal.TLI_NUMERO, 16, TOTAL_ADMON_BENEFICIOS,0)
  NEG
       ,SUM(DECODE(sal.TLI_NUMERO, 6, TOTAL_IMPORTE, 0))
      + SUM(DECODE(sal.TLI_NUMERO, 16, DECODE(ent.ENT_NUMERO_PROSA,7,0,(NVL(TOTAL_ADMON_BENEFICIOS,0) + NVL(TOTAL_IMP_BENEFICIOS, 0) + NVL(TOTAL_DCC_IMP, 0))), 0)) IMPORTE_FINAL
       ,SUM(DECODE(sal.TLI_NUMERO, 16, (NVL(TOTAL_ADMON_BENEFICIOS,0) + NVL(TOTAL_IMP_BENEFICIOS, 0) /* + NVL(TOTAL_IVA_ADMON_BENEFICIOS, 0)*/ + NVL(TOTAL_DCC_IMP, 0)), 0)) TOTAL_DCC
  FROM  supersic.SALDOS@LG_PMTU_IFO_01 sal
       ,supersic.ENTIDADES@LG_PMTU_IFO_01 ent
       ,supersic.ENTIDADES@LG_PMTU_IFO_01 ent2
 WHERE  sal.FECHA_MOVIMIENTO BETWEEN TO_DATE(?, 'dd/mm/rrrr')
                                 AND TO_DATE(?, 'dd/mm/rrrr')
 --Modificacion: Marca de INICIO SAS-ACD-C-20-5337-18
   AND  sal.ENT_NUMERO_PROSA NOT IN ( SELECT ENT_NUMERO_PROSA 
                                        FROM SUPERSIC.ENTIDADES@LG_PMTU_IFO_01
                                       WHERE (NUMERO_PROSA in (7,38)
                                          OR  ENT_NUMERO_PROSA in (7,38) ) )
 --Modificacion: Marca de FIN SAS-ACD-C-20-5337-18
   AND  sal.ENT_NUMERO_PROSA  = ent.NUMERO_PROSA
   AND  sal.TLI_NUMERO       IN (SELECT VALOR_NUMERICO
                                   FROM supersic.PARAMETROS_COMPENSACION@LG_PMTU_IFO_01
                                  WHERE NUMERO IN(106, 116)
                                )
--Modificacion: Marca de FIN SAS-ACD-C-20-5337-18
   AND  ent.ENT_NUMERO_PROSA NOT IN ( SELECT ENT_NUMERO_PROSA 
                                        FROM SUPERSIC.ENTIDADES@LG_PMTU_IFO_01 
                                       WHERE (NUMERO_PROSA in (7,38)
                                          OR  ENT_NUMERO_PROSA in (7,38) ) )
--Modificacion: Marca de FIN SAS-ACD-C-20-5337-18								
   AND  ent.ENT_NUMERO_PROSA  = ent2.NUMERO_PROSA   
   AND  ent2.NUMERO_PROSA     = DECODE(?, 0, ent2.NUMERO_PROSA, ?)
   AND  (    (sal.TLI_NUMERO =  6 AND  sal.TOTAL_IMPORTE &lt;> 0 )
    OR  (  sal.TLI_NUMERO = 16 AND ( sal.TOTAL_ADMON_BENEFICIOS     &lt;> 0
                                OR   sal.TOTAL_IMP_BENEFICIOS       &lt;> 0
                             /* OR   sal.TOTAL_IVA_ADMON_BENEFICIOS &lt;> 0 */
                                OR   sal.TOTAL_DCC_IMP              &lt;> 0
                                        )
             )

        )
 GROUP  BY ent.ENT_NUMERO_PROSA
       ,ENT2.NOMBRE
/* Termina cambio: AXIA-GGB-C-24-5065-20 12/08/2020 */
/* Termina cambio: AXIA-AVC-C-24-5065-20 13/05/2021 */
 ORDER  BY 1</property>
