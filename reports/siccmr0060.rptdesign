            <method name="beforeOpen"><![CDATA[/*
# Autor               :  Gerardo Lopez Carreno                                  #
# Compania            :  SAS S.A. DE C.V.           	                        #
# Proyecto/Procliente :  C-24-1644-22                 Fecha: 28/04/2023         #
#-----------------------------------------------------------------------------  #
*/
if (params["banco"].equals("0")){
	var concat ="       ,0 ENT ";
	var grupo ="";
	var where ="";
}else{
	var concat ="       ,ENT_NUMERO_PROSA_ADQ ENT ";
	var grupo ="         ,ENT_NUMERO_PROSA_ADQ ";
	var where ="     AND ENT_NUMERO_PROSA_ADQ IN ("+params["banco"]+") ";
}

var rsp1=
" WHERE FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+
" AND TTR_NUMERO IN (1,9,15,17,18) "+
" AND ((TLI_NUMERO IN (SELECT cl.SALIENTE FROM PMADMIN.PRSA_CAMARA_LIQ cl WHERE cl.SALIENTE IS NOT NULL) "+
"           AND CAMARA_ADQ IN ("+params["camaraAdq"]+") "+
"           AND CAMARA_EMI IN ("+params["camaraEmi"]+") "+
"           AND STTR_NUMERO IN (120, 322)) "+
"      OR (TLI_NUMERO IN (SELECT cl.ENTRANTE FROM PMADMIN.PRSA_CAMARA_LIQ cl WHERE cl.ENTRANTE IS NOT NULL) "+
"           AND CAMARA_ADQ IN ("+params["camaraAdq"]+") "+
"           AND CAMARA_EMI IN ("+params["camaraEmi"]+") "+
"           AND STTR_NUMERO IN (281, 321))) "+
" AND MARCA IN ("+params["marca"]+") ";

var rsp2=
" WHERE FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+
" AND TTR_NUMERO IN (16,19,20,21) "+
" AND ((TLI_NUMERO IN (SELECT cl.SALIENTE FROM PMADMIN.PRSA_CAMARA_LIQ cl WHERE cl.SALIENTE IS NOT NULL) "+
"           AND CAMARA_ADQ IN ("+params["camaraAdq"]+") "+
"           AND CAMARA_EMI IN ("+params["camaraEmi"]+") "+
"           AND STTR_NUMERO IN (281, 321)) "+
"      OR (TLI_NUMERO IN (SELECT cl.ENTRANTE FROM PMADMIN.PRSA_CAMARA_LIQ cl WHERE cl.ENTRANTE IS NOT NULL) "+
"           AND CAMARA_ADQ IN ("+params["camaraAdq"]+") "+
"           AND CAMARA_EMI IN ("+params["camaraEmi"]+") "+
"           AND STTR_NUMERO IN (120,322))) "+
" AND MARCA IN ("+params["marca"]+") ";

this.queryText="";

if (params["banco"].equals("0"))
this.queryText+=
" SELECT SUM(GRUPO.NO_TXN) NO_TXN "+
"       ,ROUND(SUM(GRUPO.IMPORTE),2) IMPORTE "+
"       ,0 ENT "+
"       ,'CONSOLIDADO' NOM "+
"       ,GRUPO.CONS "+
"       ,GRUPO.CONS_DESC "+
"       ,GRUPO.MARCA "+
"       ,GRUPO.MARCADS "+
" FROM( ";

this.queryText+=
" SELECT DATOS.NO_TXN "+
"       ,ROUND(DATOS.IMPORTE,2) IMPORTE "+
"       ,DATOS.ENT "+
"       ,(SELECT TRIM(descripcion) From PMADMIN.PRSA_ENTIDADES Where numero_prosa = DATOS.ENT) NOM "+
"       ,DECODE(DATOS.CONS,'15.0','14.3',DATOS.CONS) CONS "+
"       ,CASE "+
"         WHEN DATOS.CONS = '1.0'  THEN 'Ventas Débito (-)' "+
"         WHEN DATOS.CONS = '2.0'  THEN 'Devoluciones débito (+)' "+
"         WHEN DATOS.CONS = '3.0'  THEN 'Ventas Crédito (-)' "+
"         WHEN DATOS.CONS = '4.0'  THEN 'Devoluciones Crédito (+)' "+
"         WHEN DATOS.CONS = '1.1'  THEN 'Comisión Cuota Intercambio (+)' "+
"         WHEN DATOS.CONS = '2.1'  THEN 'Comisión Cuota Intercambio (-)' "+
"         WHEN DATOS.CONS = '3.1'  THEN 'Comisión Cuota Intercambio (+)' "+
"         WHEN DATOS.CONS = '4.1'  THEN 'Comisión Cuota Intercambio (-)' "+
"         WHEN DATOS.CONS = '1.2'  THEN 'Iva Comisión Cuota Intercambio (+)' "+
"         WHEN DATOS.CONS = '2.2'  THEN 'Iva Comisión Cuota Intercambio (-)' "+
"         WHEN DATOS.CONS = '3.2'  THEN 'Iva Comisión Cuota Intercambio (+)' "+
"         WHEN DATOS.CONS = '4.2'  THEN 'Iva Comisión Cuota Intercambio (-)' "+
"         WHEN DATOS.CONS = '1.3'  THEN 'Total ventas Débito (-)' "+
"         WHEN DATOS.CONS = '2.3'  THEN 'Total devoluciones débito (+)' "+
"         WHEN DATOS.CONS = '3.3'  THEN 'Total ventas Crédito (-)' "+
"         WHEN DATOS.CONS = '4.3'  THEN 'Total devoluciones Crédito (+)' "+
"         WHEN DATOS.CONS = '5.0'  THEN 'Pagos Crédito (+)' "+
"         WHEN DATOS.CONS = '6.0'  THEN 'Misceláneos A Favor (-)' "+
"         WHEN DATOS.CONS = '7.0'  THEN 'Misceláneos En Contra (-)' "+
"         WHEN DATOS.CONS = '8.0'  THEN '1° Contracargo Ventas (-)' "+
"         WHEN DATOS.CONS = '9.0'  THEN '1° Contracargo Devoluciones (+)' "+
"         WHEN DATOS.CONS = '10.0' THEN '1° Contracargo Pagos (-)' "+
"         WHEN DATOS.CONS = '11.0' THEN 'Representación Ventas (-)' "+
"         WHEN DATOS.CONS = '12.0' THEN '2° Contracargo Ventas (-)' "+
"         WHEN DATOS.CONS = '13.0' THEN '2° Contracargo Devoluciones (+)' "+
"         WHEN DATOS.CONS = '14.0' AND DATOS.IMPORTE >= 0 THEN 'Rechazo de Sintaxis +' "+
"         WHEN DATOS.CONS = '14.0' AND DATOS.IMPORTE < 0 THEN 'Rechazo de Sintaxis -' "+
"         WHEN DATOS.CONS = '14.1' THEN 'Comisión Cuota Intercambio (+)' "+
"         WHEN DATOS.CONS = '14.2' THEN 'Iva Comisión Cuota Intercambio (+)' "+
"         WHEN DATOS.CONS = '14.3' THEN 'Total Rechazo de Sintaxis (-)' "+
"         WHEN DATOS.CONS = '15.0' THEN 'Rechazo de Sintaxis (-)' "+
"        END CONS_DESC "+
"       ,DATOS.MARCA "+
"       ,(SELECT TRIM(descripcion) From PMADMIN.PRSA_MARCA Where marca = DATOS.MARCA) MARCADS "+
" FROM( "+
" SELECT NVL(SUM(NUMERO_TRANSACCIONES),0) NO_TXN "+
"       ,NVL(SUM(TOTAL_IMPORTE),0) IMPORTE "+
concat+
"       ,CASE "+
"         WHEN TIPO_TARJETA = 'D' AND TTR_NUMERO = 1 THEN '1.0' "+
"         WHEN TIPO_TARJETA = 'D' AND TTR_NUMERO = 21 THEN '2.0' "+
"         WHEN TIPO_TARJETA = 'C' AND TTR_NUMERO = 1 THEN '3.0' "+
"         WHEN TIPO_TARJETA = 'C' AND TTR_NUMERO = 21 THEN '4.0' "+
"        END CONS "+
"       ,MARCA "+
" FROM PMADMIN.PRSA_MARCA_POSICIONES "+
" INNER JOIN PMADMIN.PRSA_CAMARA_LIQ cl "+
"     ON cl.CAMARA = CAMARA_EMI "+
" WHERE FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+
"     AND CAMARA_EMI IN ("+params["camaraEmi"]+") "+
where+
"     AND TTR_NUMERO IN (1,21) "+
"     AND NVL(STTR_NUMERO,0) NOT IN (120,281,321,322) "+
"     AND TIPO_TARJETA IN ('D','C') "+
"     AND TLI_NUMERO = cl.SALIENTE "+
"     AND CAMARA_ADQ IN ("+params["camaraAdq"]+") "+
"     AND MARCA IN ("+params["marca"]+") "+
" GROUP BY CAMARA_ADQ "+
grupo+
"         ,TIPO_TARJETA "+
"         ,TTR_NUMERO "+
"         ,MARCA "+
" UNION "+
" SELECT 0 NO_TXN "+
"       ,NVL(SUM(IMPORTE_COMISION_INTERCAMB),0) IMPORTE "+
concat+
"        ,CASE "+
"         WHEN TIPO_TARJETA = 'D' AND TTR_NUMERO = 1 THEN '1.1' "+
"         WHEN TIPO_TARJETA = 'D' AND TTR_NUMERO = 21 THEN '2.1' "+
"         WHEN TIPO_TARJETA = 'C' AND TTR_NUMERO = 1 THEN '3.1' "+
"         WHEN TIPO_TARJETA = 'C' AND TTR_NUMERO = 21 THEN '4.1' "+
"        END CONS "+
"       ,MARCA "+
" FROM PMADMIN.PRSA_MARCA_POSICIONES "+
" INNER JOIN PMADMIN.PRSA_CAMARA_LIQ cl "+
"     ON cl.CAMARA = CAMARA_EMI "+
" WHERE FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+
" AND CAMARA_EMI IN ("+params["camaraEmi"]+") "+
where+
" AND TTR_NUMERO IN (1,21) "+
" AND NVL(STTR_NUMERO,0) NOT IN (120,281,321,322) "+
" AND TIPO_TARJETA IN ('D','C') "+
" AND TLI_NUMERO = cl.SALIENTE "+
" AND CAMARA_ADQ IN ("+params["camaraAdq"]+") "+
" AND MARCA = 1 "+
" GROUP BY CAMARA_ADQ "+
grupo+
"         ,TIPO_TARJETA "+
"         ,TTR_NUMERO "+
"         ,MARCA "+
" UNION "+
" SELECT 0 NO_TXN "+
"       ,NVL(SUM(IVA_COMISION_INTERCAMB),0) IMPORTE "+
concat+
"        ,CASE "+
"         WHEN TIPO_TARJETA = 'D' AND TTR_NUMERO = 1 THEN '1.2' "+
"         WHEN TIPO_TARJETA = 'D' AND TTR_NUMERO = 21 THEN '2.2' "+
"         WHEN TIPO_TARJETA = 'C' AND TTR_NUMERO = 1 THEN '3.2' "+
"         WHEN TIPO_TARJETA = 'C' AND TTR_NUMERO = 21 THEN '4.2' "+
"        END CONS "+
"       ,MARCA "+
" FROM PMADMIN.PRSA_MARCA_POSICIONES "+
" INNER JOIN PMADMIN.PRSA_CAMARA_LIQ cl "+
"     ON cl.CAMARA = CAMARA_EMI "+
" WHERE FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"', 'DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"', 'DD/MM/YYYY') "+
" AND CAMARA_EMI IN ("+params["camaraEmi"]+") "+
where+
