            <method name="beforeOpen"><![CDATA[this.queryText=
//
//-----------------------------------------------------------------------------
//--Autor               : Gerardo G. Burguete                                --
//--Compania            : AXIA Consultores S.A. de C.V.                      --
//--Proyecto/Procliente : P-02-0275-12                      Fecha: 18/12/2014--
//--Descripcion General : Servicios Adquirente POS para Banco de Occidente.  --
//--Modificacion        : Incorporar Consulta PTLF Mercados Internacionales  --
//--Marca del Cambio    : AXIA-GGB-P-02-0275-12                              --
//-----------------------------------------------------------------------------
//--Autor               : Gerardo G. Burguete                                --
//--Compania            : AXIA Consultores S.A. de C.V.                      --
//--Proyecto/Procliente : P-20-0269-14                      Fecha: 29/01/2015--
//--Descripcion General : Servicios Adquirente POS para Banco de Davivienda  --
//--Modificacion        : Incorporar Consulta PTLF Mercados Internacionales  --
//--Marca del Cambio    : AXIA-GGB-P-20-0269-14                              --
//-----------------------------------------------------------------------------
//
" SELECT PP.FECHA_PROCESO FECHA_CARGA "+
"       ,VBA.ENTIDAD_PADRE||''||VBA.DESCRIPCION||' '||VBA.BU_TX_PARENT DESCRIPCION "+
"       ,PP.TTR_NUMERO TIPO_TRAN "+
"       ,PTT.DESCRIPCION TIPO_TRAN_DESC "+
"   ,VBA.ENTIDAD_PADRE ENTIDAD_PADRE "+
"       ,SUM(PP.NUMERO_TRANSACCIONES) NUM_TXN "+
"       ,CASE "+
"          WHEN PP.TTR_NUMERO = 21 THEN -SUM(PP.TOTAL_IMPORTE) "+
"          ELSE SUM(PP.TOTAL_IMPORTE) "+
"       END IMPORTE "+
"       ,CASE "+
"          WHEN PP.TTR_NUMERO = 21 THEN -SUM(PP.IMPORTE_COMISION_INTERCAMB) "+
"          ELSE SUM(PP.IMPORTE_COMISION_INTERCAMB) "+
"       END CUOTA_COM "+
" FROM PMADMIN.PRSA_POSICIONES PP "+
" INNER JOIN PMADMIN.VW_BUS_ACQ VBA "+
"     ON VBA.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_ADQ "+
" INNER JOIN PMADMIN.PRSA_TIPOS_TRANSACCION PTT "+
"     ON PTT.NUMERO = PP.TTR_NUMERO "+
" WHERE PP.FECHA_PROCESO BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+
" AND PP.TTR_NUMERO IN (1,21,2,25,15,18,52) "+
" AND VBA.ENTIDAD_PADRE IN ("+params["bancoAdq"]+") ";
//*---------------------------------------------------------------------------//
//*Marca del Cambio : AXIA-GGB-P-20-0269-14 Inicia  la Modificacion 29/01/2015//
//*---------------------------------------------------------------------------//
//*Marca del Cambio : AXIA-GGB-P-02-0275-12 Inicia  la Modificacion 18/12/2014//
//*---------------------------------------------------------------------------//
if (params["movimiento"].equals('V'))
{
  if (params["bancoAdq"].indexOf('1302')>=0 || params["bancoAdq"].indexOf('2803')>=0 || params["bancoAdq"].indexOf('2804')>=0)
        this.queryText+=" AND (PP.ENT_NUMERO_PROSA_EMI IN (1194,1394,2594,2694,2894,2994,2803,2805) ) "+
                        " AND PP.ENT_NUMERO_PROSA_EMI <> PP.ENT_NUMERO_PROSA_ADQ  "+
                        " AND PP.TTR_NUMERO IN (1,21,2)";/*Cambio solicidato por magie 23-06-2014 */
  else
    this.queryText+=" AND PP.ENT_NUMERO_PROSA_EMI IN (1194,2594,2694,2994) ";
}

if (params["movimiento"].equals('I'))
{
  this.queryText+=" AND PP.ENT_NUMERO_PROSA_EMI IN (SELECT VBE.NUMERO_PROSA FROM PMADMIN.VW_BUS_EMI VBE WHERE VBE.TIE_NUMERO NOT IN (1,2,5,9) AND VBA.NUMERO_PROSA NOT IN (2194,2198)) ";
}
//*---------------------------------------------------------------------------//
//*Marca del Cambio : AXIA-GGB-P-20-0269-14 Termina la Modificacion 29/01/2015//
//*---------------------------------------------------------------------------//
this.queryText+=
" GROUP BY PP.FECHA_PROCESO "+
"       ,VBA.ENTIDAD_PADRE||''||VBA.DESCRIPCION||' '||VBA.BU_TX_PARENT "+
"       ,PP.TTR_NUMERO "+
"       ,PTT.DESCRIPCION "+
"   ,VBA.ENTIDAD_PADRE ";]]></method>
