            <method name="beforeOpen"><![CDATA[this.queryText=
" SELECT TITULOS.HIJO "+ 
"       ,TITULOS.TITULO "+ 
"       ,DATOS.FECHA  "+
"       ,DATOS.EMISOR  "+
"       ,DATOS.EMISOR_DESC "+ 
"       ,DATOS.EMISOR_BU  ";

if (params["banco"].equals("0"))
{
	this.queryText+=" ,0 ADQUIRENTE"+
					" ,'CONSOLIDADO' ADQUIRENTE_DESC";
}
else
{
	this.queryText+="       ,DATOS.ADQUIRENTE  "+
					"       ,DATOS.ADQUIRENTE_DESC ";
}

this.queryText+= 
"       ,DATOS.ADQUIRENTE_BU  "+
"       ,DATOS.TTR_NUMERO  "+
"       ,DATOS.TIPO_TARJETA  "+
"       ,DATOS.TXN  "+
"       ,DATOS.IMPORTE  "+
"       ,DATOS.COMISION  "+
"       ,DATOS.IVA  "+
" FROM ( "+
" SELECT PP.FECHA_LIQ FECHA "+
"       ,CASE "+
"             WHEN PP.TTR_NUMERO = 1 AND PP.TIPO_TARJETA = 'D' THEN '01' "+
"             WHEN PP.TTR_NUMERO = 21 AND PP.TIPO_TARJETA = 'D' THEN '02' "+
"             WHEN PP.TTR_NUMERO = 20 AND PP.TIPO_TARJETA = 'D' THEN '03' "+
"             WHEN PP.TTR_NUMERO = 1 AND PP.TIPO_TARJETA = 'C' THEN '04' "+
"             WHEN PP.TTR_NUMERO = 21 AND PP.TIPO_TARJETA = 'C' THEN '05' "+
"             WHEN PP.TTR_NUMERO = 20 AND PP.TIPO_TARJETA = 'C' THEN '06' "+
"             WHEN PP.TTR_NUMERO = 11 THEN '07' "+
"             WHEN PP.TTR_NUMERO = 10 THEN '08' "+
"             WHEN PP.TTR_NUMERO = 15 THEN '09' "+
"             WHEN PP.TTR_NUMERO = 16 THEN '10' "+
"             WHEN PP.TTR_NUMERO = 9 THEN '11' "+
"             WHEN PP.TTR_NUMERO = 18 THEN '12' "+
"             ELSE '99' "+
"       END ORDEN "+
"       ,CASE "+
"             WHEN PP.STTR_NUMERO IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_EMI "+
"             ELSE PP.STTR_NUMERO "+
"       END ADQUIRENTE "+
"       ,CASE "+
"             WHEN PP.STTR_NUMERO IN (95,96,97) THEN PE.DESCRIPCION "+
"             ELSE VBA.DESCRIPCION "+
"       END ADQUIRENTE_DESC "+
"       ,CASE WHEN PP.STTR_NUMERO IN (95,96,97) THEN 0 "+
"             ELSE VBA.BU_TX_ADQ "+
"       END  ADQUIRENTE_BU "+
"       ,CASE "+
"             WHEN PP.STTR_NUMERO NOT IN (95,96,97) THEN PP.ENT_NUMERO_PROSA_EMI "+
"             ELSE PP.STTR_NUMERO "+
"       END EMISOR "+
"       ,CASE "+
"             WHEN PP.STTR_NUMERO NOT IN (95,96,97) THEN PE.DESCRIPCION "+
"             ELSE VBA.DESCRIPCION "+
"       END EMISOR_DESC "+
"       ,CASE WHEN PP.STTR_NUMERO NOT IN (95,96,97) THEN 0 "+
"             ELSE VBA.BU_TX_ADQ "+
"       END EMISOR_BU "+
"       ,TO_CHAR(PP.TTR_NUMERO) TTR_NUMERO "+
"       ,TO_CHAR(PP.TIPO_TARJETA) TIPO_TARJETA "+
"       ,SUM(PP.NUMERO_TRANSACCIONES) TXN "+
"       ,ROUND(SUM(CASE "+
"         WHEN PP.TTR_NUMERO = 16 THEN -PP.TOTAL_IMPORTE "+
"         ELSE PP.TOTAL_IMPORTE "+
"        END),2) IMPORTE "+
"       ,SUM(CASE "+
"          WHEN PP.TTR_NUMERO = 16 THEN -PP.IMPORTE_COMISION_INTERCAMB "+
"          ELSE PP.IMPORTE_COMISION_INTERCAMB "+
"        END) COMISION "+
"       ,SUM(CASE "+
"          WHEN PP.TTR_NUMERO = 16 THEN -PP.IVA_COMISION_INTERCAMB "+
"          ELSE PP.IVA_COMISION_INTERCAMB "+
"        END) IVA "+
" FROM PMADMIN.PRSA_POSICIONES PP "+
" LEFT JOIN PMADMIN.VW_BUS_ACQ VBA "+
"     ON VBA.NUMERO_PROSA = PP.STTR_NUMERO "+
" INNER JOIN PMADMIN.PRSA_ENTIDADES PE "+
"     ON PE.NUMERO_PROSA = PP.ENT_NUMERO_PROSA_EMI "+
" WHERE PP.TLI_NUMERO = 33 "+//DOLARES
" AND PP.FECHA_LIQ BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+
" AND PP.TTR_NUMERO IN (1,21,20,15,9,18,16) ";

if (params["banco"].equals("0") || params["banco"]=="T")
{
	this.queryText+=" ";
}
else
{
	this.queryText+=" AND (PP.STTR_NUMERO IN ("+params["banco"]+") OR PP.ENT_NUMERO_PROSA_EMI IN ("+params["banco"]+")) ";
}

this.queryText+=
" GROUP BY PP.FECHA_LIQ "+
"       ,CASE "+
"             WHEN PP.TTR_NUMERO = 1 AND PP.TIPO_TARJETA = 'D' THEN '01' "+
"             WHEN PP.TTR_NUMERO = 21 AND PP.TIPO_TARJETA = 'D' THEN '02' "+
"             WHEN PP.TTR_NUMERO = 20 AND PP.TIPO_TARJETA = 'D' THEN '03' "+
"             WHEN PP.TTR_NUMERO = 1 AND PP.TIPO_TARJETA = 'C' THEN '04' "+
"             WHEN PP.TTR_NUMERO = 21 AND PP.TIPO_TARJETA = 'C' THEN '05' "+
"             WHEN PP.TTR_NUMERO = 20 AND PP.TIPO_TARJETA = 'C' THEN '06' "+
"             WHEN PP.TTR_NUMERO = 11 THEN '07' "+
"             WHEN PP.TTR_NUMERO = 10 THEN '08' "+
"             WHEN PP.TTR_NUMERO = 15 THEN '09' "+
"             WHEN PP.TTR_NUMERO = 16 THEN '10' "+
"             WHEN PP.TTR_NUMERO = 9 THEN '11' "+
"             WHEN PP.TTR_NUMERO = 18 THEN '12' "+
"             ELSE '99' "+
"       END "+
"       ,PP.STTR_NUMERO "+
"       ,VBA.DESCRIPCION "+
"       ,VBA.BU_TX_ADQ "+
"       ,PP.ENT_NUMERO_PROSA_EMI "+
"       ,PE.DESCRIPCION "+
"       ,PP.TTR_NUMERO "+
"       ,PP.TIPO_TARJETA "+
" UNION ALL "+
" SELECT PTRV.PTRV_FECHA_PROCESO_TRANSAC+1 FECHA "+
"       ,'13' ORDEN "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
"             ELSE PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
"       END ADQUIRENTE "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PE.DESCRIPCION "+
"             ELSE VBA.DESCRIPCION "+
"       END ADQUIRENTE_DESC "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN 0 "+
"             ELSE VBA.BU_TX_ADQ "+
"       END ADQUIRENTE_BU "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ NOT IN (95,96,97) THEN PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
"             ELSE PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
"       END EMISOR "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ NOT IN (95,96,97) THEN PE.DESCRIPCION "+
"             ELSE VBA.DESCRIPCION "+
"       END EMISOR_DESC "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ NOT IN (95,96,97) THEN 0 "+
"             ELSE VBA.BU_TX_ADQ "+
"       END EMISOR_BU "+
"       ,TO_CHAR(PTRV.PTRV_TTR_NUMERO) TTR_NUMERO "+
"       ,TO_CHAR(PTRV.PTRV_NATURALEZA_CONTABLE) TIPO_TARJETA "+
"       ,COUNT(1) TXN "+
"       ,SUM(CASE "+
"          WHEN PTRV.PTRV_TTR_NUMERO IN (21) THEN -PTRV.PTRV_IMPORTE_TOTAL_TRANSAC	"+
"          ELSE PTRV.PTRV_IMPORTE_TOTAL_TRANSAC "+
"        END) IMPORTE "+
"       ,SUM(CASE "+
"          WHEN PTRV.PTRV_TTR_NUMERO IN (21) THEN -PTRV.PTRV_IMP_CUOTA_INTERCAMBIO "+
"          ELSE PTRV.PTRV_IMP_CUOTA_INTERCAMBIO "+
"        END) COMISION "+
"       ,SUM(CASE "+
"          WHEN PTRV.PTRV_TTR_NUMERO IN (21) THEN -PTRV.PTRV_IMP_IVA_INTERCAMBIO "+
"          ELSE PTRV.PTRV_IMP_IVA_INTERCAMBIO "+
"        END) IVA "+
" FROM PMADMIN.PRSA_TXN_RECHAZADAS_VAL PTRV "+
" INNER JOIN PMADMIN.VW_BUS_ACQ VBA "+
"     ON VBA.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
" INNER JOIN PMADMIN.PRSA_ENTIDADES PE "+
"     ON PE.NUMERO_PROSA = PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
" WHERE PTRV.PTRV_FECHA_PROCESO_TRANSAC+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') "+
" AND PTRV.PTRV_TIER_CODIGO_MONEDA = 840 "+//DOLARES
" AND PTRV.PTRV_TTR_NUMERO NOT IN (53) ";

if (params["banco"].equals("0") || params["banco"]=="T")
{
	this.queryText+=" ";
}
else
{
	this.queryText+=" AND (PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN ("+params["banco"]+") OR PTRV.PTRV_ENT_NUMERO_PROSA_EMI IN ("+params["banco"]+")) ";
}

this.queryText+=
" GROUP BY PTRV.PTRV_FECHA_PROCESO_TRANSAC+1 "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
"             ELSE PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
"       END "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN PE.DESCRIPCION "+
"             ELSE VBA.DESCRIPCION "+
"       END "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ IN (95,96,97) THEN 0 "+
"             ELSE VBA.BU_TX_ADQ "+
"       END "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ NOT IN (95,96,97) THEN PTRV.PTRV_ENT_NUMERO_PROSA_EMI "+
"             ELSE PTRV.PTRV_ENT_NUMERO_PROSA_ADQ "+
"       END "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ NOT IN (95,96,97) THEN PE.DESCRIPCION "+
"             ELSE VBA.DESCRIPCION "+
"       END "+
"       ,CASE "+
"             WHEN PTRV.PTRV_ENT_NUMERO_PROSA_ADQ NOT IN (95,96,97) THEN 0 "+
"             ELSE VBA.BU_TX_ADQ "+
"       END "+
"       ,PTRV.PTRV_TTR_NUMERO "+
"       ,PTRV.PTRV_NATURALEZA_CONTABLE "+
" UNION ALL "+
" SELECT SFIT.FMI_DT FECHA_PROCESO "+ 
"       ,CASE  "+
"             WHEN VISTA.BU_TX_PARENT = 93 THEN '08' "+ 
"             ELSE '07' "+ 
"       END ORDEN  "+
"       ,CASE "+
"             WHEN VISTA.NUMERO_PROSA IN (95,96,97) THEN VISTA2.NUMERO_PROSA "+
"             ELSE VISTA.NUMERO_PROSA "+
"        END ADQUIRENTE "+
"       ,CASE "+
"             WHEN VISTA.NUMERO_PROSA IN (95,96,97) THEN VISTA2.DESCRIPCION "+
"             ELSE VISTA.DESCRIPCION "+
"        END ADQUIRENTE_DESC "+  
"       ,CASE "+
"             WHEN VISTA.NUMERO_PROSA IN (95,96,97) THEN VISTA2.BU_TX_PARENT "+
"             ELSE VISTA.BU_TX_PARENT "+
"        END ADQUIRENTE_BU "+  
"       ,CASE "+
"             WHEN VISTA.NUMERO_PROSA NOT IN (95,96,97) THEN VISTA2.NUMERO_PROSA "+
"             ELSE VISTA.NUMERO_PROSA "+
"        END EMISOR "+
"       ,CASE "+
"             WHEN VISTA.NUMERO_PROSA NOT IN (95,96,97) THEN VISTA2.DESCRIPCION "+
"             ELSE VISTA.DESCRIPCION "+
"        END EMISOR_DESC "+  
"       ,CASE "+
"             WHEN VISTA.NUMERO_PROSA NOT IN (95,96,97) THEN VISTA2.BU_TX_PARENT "+
"             ELSE VISTA.BU_TX_PARENT "+
"        END EMISOR_BU "+ 
"       ,CASE  "+
"             WHEN VISTA.BU_TX_PARENT = 93 THEN 'MISCELANEOS (EN CONTRA)' "+ 
"             ELSE 'MISCELANEOS (A FAVOR)' "+ 
"       END TTX_NUMERO "+ 
"       ,CASE  "+
"             WHEN VISTA.NOMBRE LIKE '%DB%' THEN 'D' "+ 
"             ELSE 'C'  "+
"       END TIPO_TARJETA  "+
"       ,COUNT(1) TXN  "+
"       ,SUM( "+
"             CASE   "+
"                 WHEN VISTA.BU_TX_PARENT = 93 THEN (SFIT.FMI_CHRG_AMT-SFIT.FMI_COMM_AMT-FMI_VAT_AMT) "+  
"                 ELSE SFIT.FMI_CHRG_AMT-SFIT.FMI_COMM_AMT-FMI_VAT_AMT "+
"             END "+
"           ) IMPORTE "+
"       ,0 COMISION "+ 
"       ,0 IVA  "+
" FROM SETTLEMENT.SV_FM_INT_TXN SFIT  "+
" INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA "+ 
"             UNION   "+
"             SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE "+ 
"            ) VISTA  "+
"     ON VISTA.BU = SFIT.FMI_CR_CLR_BU  "+
" INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA "+ 
"             UNION   "+
"             SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE "+ 
"            ) VISTA2  "+
"     ON VISTA2.BU = SFIT.FMI_DB_CLR_BU  "+
" WHERE SFIT.FMI_DT+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') ";

if (params["banco"].equals("0") || params["banco"]=="T")
{
	this.queryText+=" ";
}
else
{
	this.queryText+=" AND (VISTA.NUMERO_PROSA IN ("+params["banco"]+") OR VISTA2.NUMERO_PROSA IN ("+params["banco"]+")) ";
}

this.queryText+=
" AND FMI_NSI_CLR_ID = 33    "+
" AND SFIT.FMI_CHRG_CRNCY = 840 "+
" AND TRIM(FMI_OPER_ID) <> 'CZDCS030' "+ 
" GROUP BY SFIT.FMI_DT  "+
"       ,VISTA.NUMERO_PROSA "+
"       ,VISTA.DESCRIPCION  "+
"       ,VISTA.BU_TX_PARENT  "+
"       ,VISTA2.NUMERO_PROSA "+
"       ,VISTA2.DESCRIPCION  "+
"       ,VISTA2.BU_TX_PARENT "+ 
"       ,CASE  "+
"             WHEN VISTA.BU_TX_PARENT = 93 THEN 'MISCELANEOS (EN CONTRA)' "+ 
"             ELSE 'MISCELANEOS (A FAVOR)' "+ 
"       END  "+
"       ,CASE  "+
"             WHEN VISTA.NOMBRE LIKE '%DB%' THEN 'D' "+ 
"             ELSE 'C' "+ 
"       END "+
" UNION ALL "+
" SELECT PTS.PTS_CYCLE_DT FECHA_PROCESO "+
"       ,CASE "+
"             WHEN PTS.PTS_CR_CNT >0 THEN '08' "+
"             ELSE '07' "+
"       END ORDEN "+
"       ,VISTA.NUMERO_PROSA ADQUIRENTE "+
"       ,VISTA.DESCRIPCION ADQUIRENTE_DESC "+
"       ,VISTA.BU ADQUIRENTE_BU "+
"       ,VISTA2.NUMERO_PROSA EMISOR "+
"       ,VISTA2.DESCRIPCION EMISOR_DESC "+
"       ,VISTA2.BU EMISOR_BU "+
"       ,CASE "+
"             WHEN PTS.PTS_CR_CNT >0 THEN 'MISCELANEOS (EN CONTRA)' "+
"             ELSE 'MISCELANEOS (A FAVOR)' "+
"       END TTX_NUMERO "+
"       ,CASE "+
"             WHEN VISTA.NOMBRE LIKE 'DB' THEN 'D' "+
"             ELSE 'C' "+
"       END TIPO_TARJETA "+
"       ,SUM(PTS.PTS_CR_CNT+PTS.PTS_DB_CNT) TXN "+
"       ,SUM(PTS.PTS_CR_TXN_AMT+PTS.PTS_DB_TXN_AMT) IMPORTE "+
"       ,0 COMISION "+
"       ,0 IVA "+
" FROM PMADMIN.PRSA_TXN_SUMMARY PTS "+
" INNER JOIN PMADMIN.PRSA_TXN_SUMMARY PTS2 "+
"     ON PTS.PTS_CYCLE_ID = PTS2.PTS_CYCLE_ID    "+
"     AND PTS.PTS_CYCLE_DT = PTS2.PTS_CYCLE_DT "+   
"     AND PTS.PTS_NSI = PTS2.PTS_NSI    "+
"     AND PTS.PTS_NSI_CLR_ID = PTS2.PTS_NSI_CLR_ID "+   
"     AND PTS.PTS_SRCE_C = PTS2.PTS_SRCE_C    "+
"     AND PTS.PTS_PROC_CD = PTS2.PTS_PROC_CD    "+
"     AND PTS.PTS_TXN_TYP = PTS2.PTS_TXN_TYP    "+
"     AND PTS.PTS_FROM_ACCT = PTS2.PTS_FROM_ACCT   "+ 
"     AND PTS.PTS_TO_ACCT = PTS2.PTS_TO_ACCT    "+
"     AND PTS.PTS_TXN_CD_SFX = PTS2.PTS_TXN_CD_SFX "+   
"     AND PTS.PTS_CR_CNT = PTS2.PTS_DB_CNT    "+
"     AND PTS.PTS_CR_TXN_AMT = PTS2.PTS_DB_TXN_AMT    "+
"     AND PTS.PTS_CR_MRCH_FEE_AMT = PTS2.PTS_DB_MRCH_FEE_AMT    "+
"     AND PTS.PTS_CR_MRCH_IVA_AMT = PTS2.PTS_DB_MRCH_IVA_AMT    "+
"     AND PTS.PTS_CR_INTCHG_FEE_AMT = PTS2.PTS_DB_INTCHG_FEE_AMT    "+
"     AND PTS.PTS_CR_INTCHG_IVA_AMT = PTS2.PTS_DB_INTCHG_IVA_AMT "+   
"     AND PTS.PTS_DB_CNT = PTS2.PTS_CR_CNT    "+
"     AND PTS.PTS_DB_TXN_AMT = PTS2.PTS_CR_TXN_AMT    "+
"     AND PTS.PTS_DB_MRCH_FEE_AMT = PTS2.PTS_CR_MRCH_FEE_AMT    "+
"     AND PTS.PTS_DB_MRCH_IVA_AMT = PTS2.PTS_CR_MRCH_IVA_AMT    "+
"     AND PTS.PTS_DB_INTCHG_FEE_AMT = PTS2.PTS_CR_INTCHG_FEE_AMT    "+
"     AND PTS.PTS_DB_INTCHG_IVA_AMT = PTS2.PTS_CR_INTCHG_IVA_AMT "+   
" INNER JOIN PMADMIN.SZ_TC_GRP_RELATION STGR "+
"     ON STGR.TCGR_PROC_CD = PTS.PTS_PROC_CD "+
"     AND STGR.TCGR_SRCE_C = PTS.PTS_SRCE_C "+
"     AND STGR.TCGR_TO_ACCT = PTS.PTS_TO_ACCT "+
"     AND STGR.TCGR_TXN_CD_SFX = PTS.PTS_TXN_CD_SFX "+
"     AND STGR.TCGR_TXN_TYP = PTS.PTS_TXN_TYP "+
" INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA "+
"             UNION  "+
"             SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE "+
"            ) VISTA "+
"     ON VISTA.BU = PTS.PTS_CLR_BU "+
" INNER JOIN (SELECT VBA.NUMERO_PROSA, VBA.DESCRIPCION, VBA.BU_TX_ADQ BU, VBA.BU_TX_PARENT, VBA.ENTIDAD_PADRE, VBA.BU_TX_NAM_ACQ NOMBRE FROM PMADMIN.VW_BUS_ACQ VBA "+
"             UNION  "+
"             SELECT VBE.NUMERO_PROSA, VBE.DESCRIPCION, VBE.BU_TX_ISS BU, VBE.BU_TX_PARENT, VBE.ENTIDAD_PADRE, VBE.BU_TX_NAM_ISS NOMBRE FROM PMADMIN.VW_BUS_EMI VBE "+
"            ) VISTA2 "+
"     ON VISTA2.BU = PTS2.PTS_CLR_BU "+
" WHERE PTS.PTS_NSI_CLR_ID = 33 "+
" AND STGR.TCGR_GRP_ID IN (111,112) "+
" AND PTS.PTS_CRNCY_CD = 480 "+
" AND PTS.PTS_CYCLE_DT+1 BETWEEN TO_DATE('"+params["initDate"]+"','DD/MM/YYYY') AND TO_DATE('"+params["endDate"]+"','DD/MM/YYYY') ";

if (params["banco"].equals("0") || params["banco"]=="T")
{
	this.queryText+=" ";
}
else
{
	this.queryText+=" AND VISTA.NUMERO_PROSA IN ("+params["banco"]+") ";
}

this.queryText+=
" GROUP BY PTS.PTS_CYCLE_DT "+
"       ,CASE "+
"             WHEN PTS.PTS_CR_CNT >0 THEN '08' "+
"             ELSE '07' "+
"       END "+
"       ,VISTA.NUMERO_PROSA "+
"       ,VISTA.DESCRIPCION "+
"       ,VISTA.BU "+
"       ,VISTA2.NUMERO_PROSA "+
"       ,VISTA2.DESCRIPCION "+
"       ,VISTA2.BU "+
"       ,CASE "+
"             WHEN PTS.PTS_CR_CNT >0 THEN 'MISCELANEOS (EN CONTRA)' "+
"             ELSE 'MISCELANEOS (A FAVOR)' "+
"       END "+
"       ,CASE "+
"             WHEN VISTA.NOMBRE LIKE 'DB' THEN 'D' "+
"             ELSE 'C' "+
"       END "+ 
" ) DATOS "+
" INNER JOIN (SELECT '01' PAPA, '01.1' HIJO, 'Ventas Débito (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '01' PAPA, '01.2' HIJO, 'Comisión de la Cuota de Intercambio (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '01' PAPA, '01.3' HIJO, 'IVA de Comisión (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '01' PAPA, '01.4' HIJO, 'Total Ventas Débito (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '02' PAPA, '02.1' HIJO, 'Devoluciones Débito (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '02' PAPA, '02.2' HIJO, 'Comisión de la Cuota de Intercambio (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '02' PAPA, '02.3' HIJO, 'IVA de Comisión (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '02' PAPA, '02.4' HIJO, 'Total Devoluciones Débito (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '03' PAPA, '03.1' HIJO, 'Pagos Débito (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '04' PAPA, '04.1' HIJO, 'Ventas Crédito (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '04' PAPA, '04.2' HIJO, 'Comisión de la Cuota de Intercambio (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '04' PAPA, '04.3' HIJO, 'IVA de Comisión (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '04' PAPA, '04.4' HIJO, 'Total Ventas Crédito (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '05' PAPA, '05.1' HIJO, 'Devoluciones Crédito (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '05' PAPA, '05.2' HIJO, 'Comisión de la Cuota de Intercambio (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '05' PAPA, '05.3' HIJO, 'IVA de Comisión (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '05' PAPA, '05.4' HIJO, 'Total de Devoluciones Crédito (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '06' PAPA, '06.1' HIJO, 'Pagos Crédito (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '07' PAPA, '07.1' HIJO, 'Misceláneos a Favor' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '08' PAPA, '08.1' HIJO, 'Misceláneos en Contra' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '09' PAPA, '09.1' HIJO, '1° Contracargo Ventas (+)' TITULO FROM DUAL "+
" 				            UNION ALL  "+
" 				            SELECT '10' PAPA, '10.1' HIJO, '1° Contracargo Devoluciones (-)' TITULO FROM DUAL "+
" 				            UNION ALL  "+
" 				            SELECT '11' PAPA, '11.1' HIJO, 'Representación Ventas (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '12' PAPA, '12.1' HIJO, '2° Contracargo Ventas (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '13' PAPA, '13.1' HIJO, 'Rechazo de Sintaxis (+)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '13' PAPA, '13.2' HIJO, 'Comisión de la Cuota de Intercambio (-)' TITULO FROM DUAL "+ 
" 				            UNION ALL  "+
" 				            SELECT '13' PAPA, '13.3' HIJO, 'IVA de Comisión (-)' TITULO FROM DUAL "+ 
