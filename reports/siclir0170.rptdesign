    <method name="initialize"><![CDATA[var varURL="/http://10.255.10.32:8080/saciirpt2";
reportContext.setGlobalVariable("varURL",varURL);

function redondeo (valor,numDecimales){
  var multiploDiez=Math.pow(10,numDecimales);
  var k=valor*multiploDiez;
  var result;
  if(k<0)
    result=(-1.0)*Math.round(Math.abs(k))/multiploDiez;
  else
    result=Math.round(k)/multiploDiez;
  return result;

}]]></method>
