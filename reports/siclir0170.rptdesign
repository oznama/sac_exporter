                                                          , 27, (NVL(ps.TOT_IMPORTE_CASHB, 0) + NVL(ps.TOT_COM_TARIFA_CASHB, 0)) * - 1
                                                          ,  9, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0))
                                                          , 15, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0)) * - 1
                                                          , 16, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0))
                                                          , 17, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0)) * - 1
                                                          ,  3, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0))
                                                          , 22, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0)) * - 1
                                                          , 10, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0))
                                                          , 11, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0)) * -1
                                                          , 70, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0))
                                                          , 71, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0)) * - 1
                                                          , 52, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0))
                                                          , 35, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0))
                                                          , 36, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0)) * - 1
                                                          , 37, (NVL(ps.TOT_IMPORTE_CASHB, 0) - NVL(ps.TOT_COM_TARIFA_CASHB, 0))
                                                          , 0
