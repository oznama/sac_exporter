            <property name="queryText">
/* Inicia  cambio: AXIA-GGB-C-24-5065-20 12/08/2020 */              
SELECT ROUND(SUM (NVL(CIPA.IMPORTE, 0) * (DECODE(SIGN(NVL(CIPA.IMPORTE, 0)), 1, NVL(TCAM.CAMBIO_VENTA, 0), -1, NVL(TCAM.CAMBIO_COMPRA, 0)) )),2) IMPORTE
