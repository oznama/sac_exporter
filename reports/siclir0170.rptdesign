           ))           
            * - 1 IMP_PESOS
          ,'DCC_PESOS' Q
          , 'M.N.' MONEDA
          ,DECODE(ent.NUMERO_PROSA, 38,7, ent.NUMERO_PROSA) ENT_NUMERO_PROSA
          ,ent.NOMBRE
          ,tcam.CAMBIO_VENTA
          ,tcam.CAMBIO_COMPRA
--SELECT ps.*
  FROM  supersic.POSICIONES@LG_PMTU_IFO_01   ps
       ,supersic.ENTIDADES@LG_PMTU_IFO_01    ent
       ,supersic.TIPOS_CAMBIO@LG_PMTU_IFO_01 tcam
       --,ENTIDADES pa
 WHERE  ps.FECHA            BETWEEN TO_DATE(?, 'dd/mm/rrrr')
                                AND TO_DATE(?, 'dd/mm/rrrr')
   AND  ps.TLI_NUMERO            IN (46,56)
 --Modificacion: Marca de INICIO SAS-ACD-C-20-5337-18
   AND  ent.ENT_NUMERO_PROSA NOT IN ( SELECT ENT_NUMERO_PROSA                                    
                                        FROM SUPERSIC.ENTIDADES@LG_PMTU_IFO_01 
                                       WHERE NOMBRE LIKE ('%BANORTE%'))
 --Modificacion: Marca de FIN SAS-ACD-C-20-5337-18
   AND  ent.NUMERO_PROSA IN(SELECT en1.NUMERO_PROSA
                              FROM supersic.ENTIDADES@LG_PMTU_IFO_01 en1
                                  ,supersic.ENTIDADES@LG_PMTU_IFO_01 en2
                             WHERE (    en2.NUMERO_PROSA = DECODE(0, 7, 38)
                                     OR en2.NUMERO_PROSA = DECODE(0, 7, 7, 0, en2.NUMERO_PROSA, 0)
                                   )
                               AND en1.ENT_NUMERO_PROSA = en2.ENT_NUMERO_PROSA
                               AND ((en2.IND_LIQ_VISA IN ('3', '1') AND en2.IND_DCC IN('1')))
                               AND ((en1.IND_LIQ_VISA IN ('3', '1') AND en1.IND_DCC IN('1')))
                          )
   AND  (ps.ENT_NUMERO_PROSA_ADQ  =  ent.NUMERO_PROSA OR ps.ENT_NUMERO_PROSA_EMI =  ent.NUMERO_PROSA)
   AND tcam.FECHA = ps.FECHA
   AND tcam.ENT_NUMERO_PROSA = 694
   AND 1 = ?
--
 GROUP
    BY  ent.NUMERO_PROSA
       ,ent.NOMBRE
       ,tcam.CAMBIO_VENTA
       ,tcam.CAMBIO_COMPRA
UNION
SELECT  SUM(a.IMPORTE) IMPORTE
       ,a.Q
       ,a.MONEDA
       ,a.ENT_NUMERO_PROSA
       ,a.NOMBRE
       ,SUM(a.CAMBIO_VENTA ) CAMBIO_VENTA
       ,SUM(a.CAMBIO_COMPRA) CAMBIO_COMPRA
  FROM (
 SELECT SUM(NVL(cipa.importe, 0)) IMPORTE
      , 'DCC_DLLS' Q
      , 'U.S.D.' MONEDA
      , DECODE(enti.NUMERO_PROSA, 38,7, enti.NUMERO_PROSA) ENT_NUMERO_PROSA
      , enti.NOMBRE
      , 0 CAMBIO_VENTA
      , 0 CAMBIO_COMPRA
   FROM  SUPERSIC.COMPENSACIONES_INT_PARCIALES@LG_PMTU_IFO_01 cipa,
         SUPERSIC.TBL_BCO_BINICA@LG_PMTU_IFO_01               ica,
         SUPERSIC.ENTIDADES@LG_PMTU_IFO_01                    enti
  WHERE  cipa.FECHA       BETWEEN to_date(? ,'DD/MM/YYYY')
                              AND TO_DATE(? ,'DD/MM/YYYY')
    AND  cipa.NUMERO_ALTERNO    = ica.ICA
    AND  ica.ENT_NUMERO_PROSA   = enti.NUMERO_PROSA
 --Modificacion: Marca de INICIO SAS-ACD-C-20-5337-18
   AND  enti.NUMERO_PROSA NOT IN ( SELECT NUMERO_PROSA                                    
                                     FROM SUPERSIC.ENTIDADES@LG_PMTU_IFO_01 
                                    WHERE NOMBRE LIKE ('%BANORTE%'))
 --Modificacion: Marca de FIN SAS-ACD-C-20-5337-18
    AND  enti.NUMERO_PROSA NOT IN (169,1008,1009,1010,1011,1012,1013,1014,1015,1064,1507,1515,1581)
    AND  cipa.MAPR_NUMERO       =  1
    AND  cipa.TIPO_MOVIMIENTO   = 'I'
    AND  cipa.TIPO_PREFIJO      = 'P'
    AND  cipa.BIN_ICA           = 'I'
    AND  enti.IND_LIQ_VISA      = '3'
    AND  enti.IND_DCC           = '2'
    AND 0 = ?
  GROUP
     BY  enti.NUMERO_PROSA
        ,enti.NOMBRE
        ,cipa.FECHA
--
) a
GROUP
   BY  a.Q
      ,a.MONEDA
      ,a.ENT_NUMERO_PROSA
      ,a.NOMBRE
UNION
SELECT  SUM(a.IMPORTE) IMPORTE
       ,a.Q
       ,a.MONEDA
       ,a.ENT_NUMERO_PROSA
       ,a.NOMBRE
       ,SUM(a.CAMBIO_VENTA ) CAMBIO_VENTA
       ,SUM(a.CAMBIO_COMPRA) CAMBIO_COMPRA
  FROM (
--
 SELECT  SUM(NVL(cipa.importe, 0) * (DECODE(SIGN(NVL(cipa.importe, 0)), 1, NVL(tcam.cambio_venta, 0),-1, NVL(tcam.cambio_compra, 0)))) IMPORTE
       ,'DCC_PESOS' Q
       ,'M.N.' MONEDA
        ,DECODE(enti.NUMERO_PROSA, 38,7, enti.NUMERO_PROSA) ENT_NUMERO_PROSA
        ,enti.NOMBRE
        ,0 CAMBIO_VENTA
        ,0 CAMBIO_COMPRA
--
--Select *
   FROM  SUPERSIC.COMPENSACIONES_INT_PARCIALES@LG_PMTU_IFO_01 cipa,
         SUPERSIC.TBL_BCO_BINICA@LG_PMTU_IFO_01               ica,
         SUPERSIC.ENTIDADES@LG_PMTU_IFO_01                    enti,
         SUPERSIC.TIPOS_CAMBIO@LG_PMTU_IFO_01                 tcam,
         SUPERSIC.PARAMETROS_COMPENSACION@LG_PMTU_IFO_01      pcom
  WHERE  cipa.FECHA       BETWEEN to_date(? ,'DD/MM/YYYY')
                              AND TO_DATE(? ,'DD/MM/YYYY')
    AND  cipa.NUMERO_ALTERNO    = ica.ICA
 --Modificacion: Marca de INICIO SAS-ACD-C-20-5337-18
    AND  enti.ENT_NUMERO_PROSA NOT IN ( SELECT ENT_NUMERO_PROSA                                    
                                          FROM SUPERSIC.ENTIDADES@LG_PMTU_IFO_01 
                                         WHERE NOMBRE LIKE ('%BANORTE%'))
 --Modificacion: Marca de FIN SAS-ACD-C-20-5337-18
    AND  ica.ENT_NUMERO_PROSA   = enti.NUMERO_PROSA
    AND  enti.NUMERO_PROSA NOT IN (169,1008,1009,1010,1011,1012,1013,1014,1015,1064,1507,1515,1581)
    AND  pcom.NUMERO            = 201
    AND  cipa.MAPR_NUMERO       =  1
    AND  cipa.TIPO_MOVIMIENTO   = 'I'
    AND  cipa.TIPO_PREFIJO      = 'P'
    AND  cipa.BIN_ICA           = 'I'
    AND  tcam.ENT_NUMERO_PROSA  = pcom.VALOR_NUMERICO
    AND  tcam.FECHA             = cipa.FECHA
    AND  enti.IND_LIQ_VISA      =  3
    AND  enti.IND_DCC           =  2
    AND  1 = ?
--
  GROUP
     BY  enti.NUMERO_PROSA
        ,enti.NOMBRE
        ,cipa.FECHA
--
) a
GROUP
   BY  a.Q
      ,a.MONEDA
      ,a.ENT_NUMERO_PROSA
